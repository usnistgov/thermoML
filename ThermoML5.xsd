                                                  <xsd:documentation>measured unit of variable/property</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="mol K-1 kg-1"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                  </xsd:element>
                                                  <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                  <xsd:element name="method">
                                                  <xsd:annotation>
                                                  <xsd:documentation>Method for phase equilibria determination.</xsd:documentation>
