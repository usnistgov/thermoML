        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="component" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>component linked to property </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="By X=Y"/>
                            <xsd:enumeration value="By T or P extreme"/>
                            <xsd:enumeration value="Chromatography"/>
                            <xsd:enumeration value="Spectrophotometry"/>
                            <xsd:enumeration value="Density calibration data"/>
                            <xsd:enumeration value="Index of refraction calibration data"/>
                            <xsd:enumeration value="Speed of sound calibration data"/>
                            <xsd:enumeration value="Calculated by Gibbs-Duhem equation"/>
                            <xsd:enumeration value="Titration method"/>
                            <xsd:enumeration value="Phase equilibration"/>
                            <xsd:enumeration value="Derived from phase diagram analysis"/>
                            <xsd:enumeration value="Photoacoustic method"/>
                            <xsd:enumeration value="Other"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="osmoticCoefficientType">
        <xsd:annotation>
            <xsd:documentation>used for osmotic coefficient IUPAC: Quantity characterizing the
                deviation of the solvent from ideal behaviour referenced to Raoult's
                law.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="solute" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>Solute component linked to property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" name="solvent" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>solvent component(s) linked to property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="isopiestic method"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="compositionBasis">
            <xsd:annotation>
                <xsd:documentation>identification of composition bases (molal or amount fraction)</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="molality basis"/>
                    <xsd:enumeration value="amount fraction basis"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="ostwaldCoefficientType">
        <xsd:annotation>
            <xsd:documentation>used for ostwald coefficient, </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="solute" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>solute component  linked to property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" name="solvent" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>solvent component(s) linked to property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string"> </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="definition" use="required">
            <xsd:annotation>
                <xsd:documentation>definition of ostwald coefficient choice: liquid is in equilibrium solution or pure liquid.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="liquid is equilibrium solution"/>
                    <xsd:enumeration value="liquid is pure liquid"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="otherRefractiveIndexType">
        <xsd:annotation>
            <xsd:documentation>used for refractive index other wavelength, excess refractive
                index with other wavelength</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="wavelength" type="tml:waveLengthType" minOccurs="0">
                <!-- needs to be set for wavelength-->
                <xsd:annotation>
                    <xsd:documentation>wavelength associated with
                        measurement</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Standard Abbe refractometry"/>
                            <xsd:enumeration value="Interferometry (monochromatic)"/>
                            <xsd:enumeration value="Dipping refractometry"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="partialPressureType">
        <xsd:annotation>
            <xsd:documentation>used for partial pressure, fugacity</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="Pa">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Pa"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="component" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>component linked to property </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="surroundingAtmosphere" type="tml:chemicalRefType"
                maxOccurs="unbounded" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>component(s) in gaseous mixture linked to property </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="knudsen effusion mass spectroscopy"/>
                            <xsd:enumeration value="calculated from Knudsen effusion weight loss"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="partialPressureDefn" use="required">
            <xsd:annotation>
                <xsd:documentation>description of conditions: at 1 atm or at other pressure.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="at 1 atm"/>
                    <xsd:enumeration value="at other Pressure"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="permittivityType">
        <xsd:annotation>
            <xsd:documentation>used for excess relative permittivity at zero frequency,
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="F/m">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="F/m"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Parallel plate capacitor"/>
                            <xsd:enumeration value="Coaxial cylinder capacitor"/>
                            <xsd:enumeration value="Cross capacitor"/>
                            <xsd:enumeration value="Reentrant cavity resonator"/>
                            <xsd:enumeration value="Coaxial line"/>
                            <xsd:enumeration value="Wave guide"/>
                            <xsd:enumeration value="Far infrared free space"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="phaseTransitionTemperatureType">
        <xsd:annotation>
            <xsd:documentation>used for phase transition temperature</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="K">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="K"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Calorimetry measurements"/>
                            <xsd:enumeration
                                value="Characterized using X-ray absorption spectroscopy"/>
                            <xsd:enumeration value="Characterized using Neutron diffraction"/>
                            <xsd:enumeration value="Characterized using X-ray diffraction"/>
                            <xsd:enumeration value="Characterized using X-ray tomography"/>
                            <xsd:enumeration value="Determined from cooling curves"/>
                            <xsd:enumeration value="Determined from DSC"/>
                            <xsd:enumeration value="Determined from DTA"/>
                            <xsd:enumeration value="Determined from Dilatometry"/>
                            <xsd:enumeration value="Determined from Interferometry"/>
                            <xsd:enumeration value="Determined from resistivity changes"/>
                            <xsd:enumeration value="Determined from permeability changes"/>
                            <xsd:enumeration value="Determined from volume changes"/>
                            <xsd:enumeration value="Metallographic inspection- Optical microsciopy"/>
                            <xsd:enumeration
                                value="Metallographic inspection- Tranmission electron microscopy"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
            <xsd:sequence>
                <xsd:element name="equilibriumPhases">
                    <xsd:annotation>
                        <xsd:documentation>list of n>1 phases involved in
                            transformation</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="phase" minOccurs="2" maxOccurs="unbounded"
                                type="tml:phaseRefType">
                                <xsd:annotation>
                                    <xsd:documentation>link to equilibrium phase(s)</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element minOccurs="0" name="zeroPhaseFraction">
                    <xsd:annotation>
                        <xsd:documentation>list of phase(s) with fraction approaching zero,
                            note: 0 phases indicated binary invariant reaction where no zero phase
                            fields are identified.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="phase" minOccurs="0" maxOccurs="unbounded"
                                type="tml:phaseRefType">
                                <xsd:annotation>
                                    <xsd:documentation>link to zero phase fraction phase(s)</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="phaseTransitionType">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="normalMelting" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="normalBoiling" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="sublimation" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="liquidus" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="solidus" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="solvus" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="eutecticTransformation" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="eutectoidTransformation" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="peritecticTransformation" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="peritectoidTransformation" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="monotecticTransformation" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="monotectoidTransformation" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="invariantReactionTransition" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="orderingTransition" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="lambdaSuperconductiveTransition" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="shearTransition" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="glassTransitition" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="magneticTransition" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="secondOrderPhaseTransition" type="tml:phaseFieldType"> </xsd:element>
            <xsd:element name="firstOrderPhaseTransition" type="tml:phaseFieldType"> </xsd:element>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="presssureCoefficientOfMolarEnthalpyType">
