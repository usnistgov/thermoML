    <xsd:complexType name="phaseType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="type">
                <xsd:annotation>
                    <xsd:documentation>type of phase: liquid, solid gas, including more complex types, etc.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="crystal"/>
                        <xsd:enumeration value="gas"/>
                        <xsd:enumeration value="glass"/>
                        <xsd:enumeration value="liquid"/>
                        <xsd:enumeration value="quasicrystal"/>
                        <xsd:enumeration value="unknown solid"/>
                        <xsd:enumeration value="unknown/supercritical fluid"/>
                        <xsd:enumeration value="inert media at 1 atm"/>
                        <xsd:enumeration value="cholesteric liquid crystal"/>
                        <xsd:enumeration value="nematic liquid crystal"/>
                        <xsd:enumeration value="smectic liquid crystal"/>
                        <xsd:enumeration value="unknown-type liquid crystal"/>
                        <xsd:enumeration value="gel"/>
                        <xsd:enumeration value="subgel"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="commonName" maxOccurs="unbounded" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Name of phase</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="crystalLattice" type="tml:crystalLatticeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>determined crystal structure: cubic, rhombahedral, triclinic, etc.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="magneticState" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>magnetic state (ferro, para, non, anti, complex, magnetism)</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="non-magnetic"/>
                        <xsd:enumeration value="paramagnetic"/>
                        <xsd:enumeration value="ferromagnetic"/>
                        <xsd:enumeration value="antiferromagnetic"/>
                        <xsd:enumeration value="complex magnetism"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="metastable" type="xsd:boolean" minOccurs="0" default="false">
                <xsd:annotation>
                    <xsd:documentation>boolean indicator to indicate instability of phase</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="molecularFormula" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>For compounds consisting of discrete molecules, a formula according with the relative molecular mass (or the structure)</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="component" maxOccurs="unbounded" minOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation>components in chemical formula</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="chemical" type="tml:chemicalRefType">
                                        <xsd:annotation>
                                            <xsd:documentation>link to chemical/component</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="amount" type="xsd:float">
                                        <xsd:annotation>
                                            <xsd:documentation>molecular amount</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="compositionBasis" default="number of molecules">
                        <xsd:annotation>
                            <xsd:documentation>attribute used to identify the basis for composition used with data (No. molecules, mole fraction, weight fraction)</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="number of molecules"/>
                                <xsd:enumeration value="mole fraction"/>
                                <xsd:enumeration value="mass fraction"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:attribute>
                </xsd:complexType>
