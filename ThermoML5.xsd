                        <xsd:documentation>method used for measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Inductive pick up coils"/>
                            <xsd:enumeration value="Vibrating sample magnetometer"/>
                            <xsd:enumeration value="Pulsed field extraction"/>
                            <xsd:enumeration value="Torque magnetometer"/>
                            <xsd:enumeration value="Faraday magnetometer"/>
                            <xsd:enumeration value="Optical magnetometer"/>
                            <xsd:enumeration value="Hall effect magnetometer"/>
                            <xsd:enumeration value="Superconducing quantum interference device"/>
