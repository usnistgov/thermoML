    <xsd:complexType name="bunsenCoefficientType">
        <xsd:annotation>
            <xsd:documentation>used for Bunsen coefficient, </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                        <xsd:enumeration value=""/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string"> </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="solute" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>solute component  linked to property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" name="solvent" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>solvent component(s) linked to property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="definition" use="required">
            <xsd:annotation>
                <xsd:documentation>definition of Bunsen coefficient reference. either reference liquid is in equilibrium solution or pure liquid.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="solution reference"/>
