                                                  <xsd:enumeration value="derived, from PVT data"/>
                                                  <xsd:enumeration
                                                  value="derived, from rectilinear diameter"/>
                                                  <xsd:enumeration value="derived, from second law"/>
                                                  <xsd:enumeration
