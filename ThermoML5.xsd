    <xsd:complexType name="relativeMolarVolumeType">
        <!--unsure about some methods-->
        <xsd:annotation>
            <xsd:documentation>type for: apparent molar volume, relative partial molar volume,
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="m^3/mol">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="m^3/mol"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="solute" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>link to solute component associated with property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" name="solvent" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>link to solvent component(s) associated with property</xsd:documentation>
