        </xsd:keyref>
        <xsd:keyref name="system-apparentMolarEntropy-phaseKeyRef" refer="tml:phaseIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference for phase id for apparent molar entropy</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/property/entropy/apparentMolarEntropy/phase"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-excessMolarEntropy-phaseKeyRef" refer="tml:phaseIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference for phase id for excess molar entropy</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/property/entropy/excessMolarEntropy/phase"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-partialMolarEntropy-phaseKeyRef" refer="tml:phaseIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference for phase id for partial molar entropy</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/property/entropy/partialMolarEntropy/phase"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-relativePartialMolarEntropy-phaseKeyRef" refer="tml:phaseIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference for phase id for relative partial molar entropy</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/entropy/relativePartialMolarEntropy/phase"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-standardStateMolarEntropy-phaseKeyRef" refer="tml:phaseIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference for phase id for standard state molar entropy</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/entropy/standardStateMolarEntropy/phase"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-molarEntropy-phaseKeyRef" refer="tml:phaseIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference for phase id for  molar entropy</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/property/entropy/molarEntropy/phase"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-molarEntropyOfTransitionOrFusion-phaseKeyRef"
            refer="tml:phaseIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference for phase id for  molar entropy of transition or fusion</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/entropy/molarEntropyOfTransitionOrFusion/phase"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-molarEntropyOfVaporizationOrSublimation-phaseKeyRef"
            refer="tml:phaseIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference for phase id for molar enthalpy of vaporization or sublimation</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/entropy/molarEntropyOfVaporizationOrSublimation/phase"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>

        <!-- ******************************** -->
        <!-- phaseField key references -->
        <!-- ******************************** -->

        <xsd:keyref name="dataset-datapoint-phaseFieldKeyRef" refer="tml:phaseFieldIDKey">
            <xsd:annotation>
                <xsd:documentation>reference key for phase(s) for which the condition of property describes</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/point/phaseField"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>

        <!-- ******************************** -->
        <!-- referenceDetail key references -->
        <!-- ******************************** -->

        <xsd:keyref name="dataset-fixedConditionReference-refDetailKeyRef"
            refer="tml:referenceDetailIDKey">
            <xsd:annotation>
                <xsd:documentation>Key reference for the id of a reference state defined for a fixed condition</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/fixedConditions/condition/refState"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-variableConditionReference-refDetailKeyRef"
            refer="tml:referenceDetailIDKey">
            <xsd:annotation>
                <xsd:documentation>Key reference for the id of a reference state defined for a fixed condition</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/variableConditions/condition/refState"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-propertyXRef-refDetailKeyRef" refer="tml:referenceDetailIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to relate a properties refrence details</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/property/xRef"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>



        <!-- ******************************** -->
        <!-- equation dataset key references -->
        <!-- ******************************** -->

        <xsd:keyref name="dataset-equationProperty-equationDataNumberKeyRef"
            refer="tml:equationDataSetIDKey">
            <xsd:annotation>
                <xsd:documentation>a unique number for each instance of the pure or mixeture data block, used for external linking in equation representation. ThermoML 4.0 was nPureOrMixtureDataNumber</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/equation/equationProperty/dataSetRef"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-equationCondition-equationDataNumberKeyRef"
            refer="tml:equationDataSetIDKey">
            <xsd:annotation>
                <xsd:documentation>a unique number for each instance of the pure or mixeture data block, used for external linking in equation representation. ThermoML 4.0 was nPureOrMixtureDataNumber</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/equation/equationCondition/dataSetRef"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-equationVariable-equationDataNumberKeyRef"
            refer="tml:equationDataSetIDKey">
            <xsd:annotation>
                <xsd:documentation>a unique number for each instance of the pure or mixeture data block, used for external linking in equation representation. ThermoML 4.0 was nPureOrMixtureDataNumber</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/equation/equationVariable/dataSetRef"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>

        <!-- ******************************** -->
        <!-- reaction equation key references -->
        <!-- ******************************** -->

        <xsd:keyref name="dataset-equationProperty-reactionDataNumberKeyRef"
            refer="tml:reactionDataSetIDKey">
            <xsd:annotation>
                <xsd:documentation>ThermoML 4.0 was nReaction DataNumber</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/equation/equationProperty/reactionDataSetRef"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-equationConstant-reactionDataNumberKeyRef"
            refer="tml:reactionDataSetIDKey">
            <xsd:annotation>
                <xsd:documentation>ThermoML 4.0 was nReaction DataNumber</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/equation/equationCondition/reactionDataSetRef"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-equationVariable-reactionDataNumberKeyRef"
            refer="tml:reactionDataSetIDKey">
            <xsd:annotation>
                <xsd:documentation>ThermoML 4.0 was nReaction DataNumber</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/equation/equationVariable/reactionDataSetRef"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>


        <!-- ************************************************************************** -->
        <!--      property and variable/condition ID key references                     -->
        <!-- ************************************************************************** -->

        <xsd:keyref name="dataset-equationCondition-conditionKeyRef" refer="tml:conditionIDKey">
            <xsd:annotation>
                <xsd:documentation>key ref to property id # thermoML 4.0 was nPropNumber</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/equation/equationCondition/conditionRef"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-equationVariable-variableConditionKeyRef"
            refer="tml:varConditionIDKey">
            <xsd:annotation>
                <xsd:documentation>key ref to property id # thermoML 4.0 was nPropNumber</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/equation/equationVariable/conditionRef"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-equationProperty-propertyrKeyRef" refer="tml:propertyIDKey">
            <xsd:annotation>
                <xsd:documentation>key ref to property id # thermoML 4.0 was nPropNumber</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/equation/equationProperty/propertyref"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>

    </xsd:element>

    <!-- **********************************-->
    <!--         DATAREPORT definition     -->
    <!-- **********************************-->

    <xsd:complexType name="dataReportType">
        <xsd:annotation>
            <xsd:documentation>Type information for root container</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="citation" type="tml:citationType" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Elements to convey provenance about communicated
                        data</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="chemical" type="tml:chemicalType" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Elements of chemicals referenced in the
                        data</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="phase" type="tml:phaseType" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Elements to identify phases (Strukturberict,
                        etc.)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="phaseField" type="tml:phaseFieldType" minOccurs="0"
                maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>region</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="system" type="tml:systemType" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Descriptions for reactions and processing-sample-component relationships (mostly header information)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataset" type="tml:datasetType" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>The measurements of a system reported in a
                        citation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <!-- *************************************-->
    <!--          CITATION definition         -->
    <!-- *************************************-->
    <xsd:complexType name="citationType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="TRCRefID" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>TRC Reference Identifier</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="yearPub">
                            <xsd:annotation>
                                <xsd:documentation>Integer year of publication</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:integer">
                                    <xsd:pattern value="[\-+]?[0-9]+"/>
                                    <xsd:totalDigits value="4"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="author1">
                            <xsd:annotation>
                                <xsd:documentation>First 3 characters of Author 1 last
                                    name</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:maxLength value="3"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="author2" default="" minOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation>First 3 characters of Author 2 last
                                    name</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:maxLength value="3"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="authorN" type="xsd:integer">
                            <xsd:annotation>
                                <xsd:documentation>Integer identifier to distinguish documents from
                                    conflicts</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="abstract" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>An abstract of the publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="author" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Author of publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CASCitation" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The Chemical Abstracts Service citation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="documentType" minOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>The type of publication (book, journal
                        etc.)</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="book"/>
                        <xsd:enumeration value="journal"/>
                        <xsd:enumeration value="report"/>
                        <xsd:enumeration value="patent"/>
                        <xsd:enumeration value="thesis"/>
                        <xsd:enumeration value="conferenceProceedings"/>
                        <xsd:enumeration value="archivedDocument"/>
                        <xsd:enumeration value="personalCorrespondence"/>
                        <xsd:enumeration value="publishedTranslation"/>
                        <xsd:enumeration value="unspecified"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="DOI" type="xsd:anyURI" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>digital object identifier string</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="keyword" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>List of keywords provided with
                        publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="language" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Language of publication</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="chinese"/>
                        <xsd:enumeration value="english"/>
                        <xsd:enumeration value="french"/>
                        <xsd:enumeration value="german"/>
                        <xsd:enumeration value="japanese"/>
                        <xsd:enumeration value="polish"/>
                        <xsd:enumeration value="russian"/>
                        <xsd:enumeration value="otherLanguage"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="sourceType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The source status type for a citation (original, Chemical
                        abstract, etc.)</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="original"/>
                        <xsd:enumeration value="chemicalAbstracts"/>
                        <xsd:enumeration value="other"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="title" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Title of the publication, section/chapter title,  or patent</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="translationOf" minOccurs="0" type="tml:citationRefType">
                <xsd:annotation>
                    <xsd:documentation>link to document that was originally translated</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="URL" type="xsd:anyURI" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>URL to the publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="yearPublished" type="xsd:gYear" minOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Publication year</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0">
                <xsd:element name="book">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="bookTitle" type="xsd:string" minOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation>Title of the book</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="chapter" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Chapter number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="edition" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Edition number of the
                                        book</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="editor" type="xsd:string" minOccurs="0"
                                maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Editor(s) of the book</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ISBN" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The International Standard Book
                                        Number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="publisher" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Publisher name</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="publisherLocation" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Location of publishing house</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="pages" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Page range or number of pagest total.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="volume" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Volume number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="conferenceProceed">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="conferenceTitle" type="xsd:string" minOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation>Company, institution, or conference name</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="conferenceOrganizer" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>conference organizer name</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="conferenceStartDate" type="xsd:date" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>conference start date</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="conferenceEndDate" type="xsd:date" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>conference end date</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="conferenceStartMonth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>conference start date</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:integer">
                                        <xsd:minInclusive value="1"/>
                                        <xsd:maxInclusive value="12"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="conferenceEndMonth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>conference end date</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:integer">
                                        <xsd:minInclusive value="1"/>
                                        <xsd:maxInclusive value="12"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="location" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to a location</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="paperIDNum" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Identification number, e.g. a paper number </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="pages" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Page range</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="volume" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>proceedings volume number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="journal">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="CODEN" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The CODEN identification of the
                                        journal</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:length value="6"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="erratumOf" type="tml:citationRefType">
                                <xsd:annotation>
                                    <xsd:documentation>link to document that errata belongs to</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="IDNum" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Identification number, e.g. a paper number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ISSN" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The International Standard Subscription
                                        Number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="issue" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Issue number, usually indicates
                                        month</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="journalName" type="xsd:string" minOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation>Name of the publication. (Nature, Scince,
                        etc.)</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="pages" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Page range</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="volume" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Volume number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="patent">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="patentIDNum" type="xsd:string" minOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation>Identification number, e.g. a patent number </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="patentCountry" type="xsd:string" minOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation>location of patent issuance</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="patentHolder" type="xsd:string" minOccurs="0"
                                maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Patent holder name</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="patentStartDate" type="xsd:date" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>patent start date</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="patentEndDate" type="xsd:date" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>patent end date</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="report">
                    <xsd:complexType>
                        <xsd:sequence maxOccurs="unbounded">
                            <xsd:element name="IDNum" minOccurs="1" maxOccurs="1" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation>Identification number, e.g. report or document number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="documentOrigin" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Company, institution, where report filed</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="pages" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Number of pagest total or page range.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="thesis">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="degree" minOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation>Academic degree designation, e.g., MS or
                                        PhD</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="PhD"/>
                                        <xsd:enumeration value="Master"/>
                                        <xsd:enumeration value="Bachelor"/>
                                        <xsd:enumeration value="other"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="institution" type="xsd:string" minOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation>Academic degree granting
                                        institution</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="UMIPublicationNumber" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>University Microfilms International
                                        Publication Number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="location" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>location of degree giving institution</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:choice>
            <xsd:element minOccurs="0" name="note" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>any notes about reference that are not captured anywhere else.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:positiveInteger" use="required">
            <xsd:annotation>
                <xsd:documentation>citation id#, only useful for within this xml, non-transferable between xml documents.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <!-- *************************************-->
    <!--          CHEMICAL definition         -->
    <!-- *************************************-->

    <xsd:complexType name="chemicalType">
        <xsd:annotation>
            <xsd:documentation>Material component characteristics that serves to identify the
                component</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="CASName" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>name as given by the chemical abstract
                        service</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="commonName" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:appinfo>Common name, string</xsd:appinfo>
                    <xsd:documentation>Common name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IUPACName" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:appinfo>IUPAC name, string</xsd:appinfo>
                    <xsd:documentation>International Union of Physics and Applied Chemistry
                        name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="molecularFormula" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:appinfo>Molecular formula, string</xsd:appinfo>
                    <xsd:documentation>Chemical molecular formula</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="organizationID" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>organization specific identifiers</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="organizationIdentifier" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>identifying string for chemcial</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="organization" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>organization associated with organizationIdentifier</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="pubChemID" type="xsd:integer" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Chemical identificaiton number assigned via PubChem database</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="regNum" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Registery chemical identifier 
(Organizational or CASRN-Chemical Abstract Service Registry Number)</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="CASRNum" type="xsd:integer" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Chemical Abstract Services Registry Number (CASRN)</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:sequence minOccurs="0">
                            <xsd:element name="organizaitonGivenNumber" type="xsd:integer"
                                minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Registry number as given by
                                        organization</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="organizationName" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Organization name that designated
                                        organization number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="speciationState" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>comprehensive compositional description for equilibrium or single species (subspecies ) associated with a compound.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="equilibrium"/>
                        <xsd:enumeration value="single species"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="standardInChI" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Standard International CHemical Identifier key assigned by IUPAC</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="standardInChIKey" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>hashed standard INternational CHemical Identifier key assigned by IUPAC</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SMILES" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:appinfo>SMILES notation, string</xsd:appinfo>
                    <xsd:documentation>SMILES notation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0">
                <xsd:element name="ion">
                    <xsd:complexType>
                        <xsd:annotation>
                            <xsd:documentation>Ion-specific information</xsd:documentation>
                        </xsd:annotation>
                        <xsd:sequence>
                            <xsd:element name="charge" type="xsd:integer">
                                <xsd:annotation>
                                    <xsd:documentation>charge associated with ion</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="biomaterial">
                    <xsd:annotation>
                        <xsd:documentation>biomaterial specific identification elements</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:annotation>
                            <xsd:documentation>Bio species information</xsd:documentation>
                        </xsd:annotation>
                        <xsd:sequence>
                            <xsd:element name="ECNumber" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Enzyme commission number, as assigned by the Nomenclature Committee of the International Union of Biochemistry and Molecular Biology(NC-IUBMB) in consultation with the IUCRC_IUMBM</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="PDBIdentifier" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Protein Data Bank (PDB) identifier.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="polymer">
                    <xsd:annotation>
                        <xsd:documentation>polymer specific identification elements</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:annotation>
                            <xsd:documentation>Polymer-specific information</xsd:documentation>
                        </xsd:annotation>
                        <xsd:sequence>
                            <xsd:element name="numberAvgMolMass" type="xsd:float" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Number average molecular mass,
                                        kg/kmol</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="peakAvgMolMass" type="xsd:float" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Peak average molecular mass,
                                        kg/kmol</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="viscosityAvgMolMass" type="xsd:float" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Viscosity average molecular mass,
                                        kg/kmol</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="massAvgMolMass" type="xsd:float" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Weight average molecular mass,
                                        kg/kmol</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="zAvgMolMass" type="xsd:float" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Z average molecular mass,
                                        kg/kmol</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="molarMassDispersity" type="xsd:float" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Ratio of the mass-average molar mass, relative molecular mass, or molecular weight, to the number-average molar mass, relative molecular mass, or molecular weight.

\overline{M_w}/\overline{M_n}</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="degOfPolymerizationDispersity" type="xsd:float"
                                minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Ratio of the mass-average degree of polymerization, \overline[X_w], to the number-average degree of polymerization, \overline{X_n}

\overline{M_w}/\overline{M_n}</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:positiveInteger" use="required">
            <xsd:annotation>
                <xsd:documentation>chemical id#, an index to link elements or compound(s) to data. Only useful for within this XML, non-transferable between XML documents.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <!-- *************************************-->
    <!--          SYSTEM  definition          -->
    <!-- *************************************-->

    <xsd:complexType name="systemType">
        <xsd:sequence maxOccurs="1">
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:choice>
                <xsd:element name="sample" type="tml:sampleType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>physical specimen details and
                        characterization</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="multiComponent" type="tml:multiComponentType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Components in system (i.e. methanol and water, graphite, iron
                        and nickel.)</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="processing" type="tml:processType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Any processing done to specimens prior to
                        testing</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="reaction" type="tml:reactionType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>reaction information (participants, buffers, reaction IDs,
                        etc.)</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:positiveInteger" use="required">
            <xsd:annotation>
                <xsd:documentation>system id#, only useful for within this XML, non-transferable between XML documents.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="multiComponentType">
        <xsd:sequence>
            <xsd:annotation>
                <xsd:documentation>sequence of general identifier and component(s) associated with a multi-component system/alloy/etc.</xsd:documentation>
            </xsd:annotation>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="alloyName" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>for metallic systems, different taxonomies for alloys</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element minOccurs="0" name="ASMEName" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation/>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="ASTMName">
                            <xsd:annotation>
                                <xsd:documentation/>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:choice>
                                    <xsd:element name="string" type="xsd:string"/>
                                    <xsd:element name="coded">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="firstLetter">
                                                  <xsd:annotation>
                                                  <xsd:documentation>letter indicates material
                                                system</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:pattern value="[A-Z]"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="sequentialNumber">
                                                  <xsd:annotation>
                                                  <xsd:documentation>number associated with
                                                alloy</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:integer">
                                                  <xsd:minInclusive value="1"/>
                                                  <xsd:maxInclusive value="9999"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="grade">
                                                  <xsd:annotation>
                                                  <xsd:documentation>chemical composition
                                                descriptor</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:pattern value="([a-zA-Z0-9])*"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="standardYear" type="xsd:gYear">
                                                  <xsd:annotation>
                                                  <xsd:documentation>typically indicated as (XXXX) as the
                                                year of standard associated with the ASTM
                                                standard</xsd:documentation>
                                                  </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="type">
                                                  <xsd:annotation>
                                                  <xsd:documentation>deoxidation practice
                                            </xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:pattern value="([a-zA-Z0-9])*"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="class">
                                                  <xsd:annotation>
                                                  <xsd:documentation>other characteristics (surface finish,
                                                strength level etc.)</xsd:documentation>
                                                  </xsd:annotation>
                                                  <xsd:simpleType>
                                                  <xsd:restriction base="xsd:string">
                                                  <xsd:pattern value="([a-zA-Z0-9])*"/>
                                                  </xsd:restriction>
                                                  </xsd:simpleType>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:choice>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="AFNORName" maxOccurs="1" minOccurs="0" type="xsd:string"> </xsd:element>
                        <xsd:element name="ANSIName" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="BSIName" type="xsd:string" minOccurs="0"/>
                        <xsd:element minOccurs="0" name="DINTypeName" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation/>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="DINNumber" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation/>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="IECName" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="ISOName" type="xsd:string" minOccurs="0"/>
                        <xsd:element minOccurs="0" name="MILName" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation/>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="UNSName" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>
</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="TradeName" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation/>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="component" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>components that make up the system/specimen</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="label" type="xsd:string" minOccurs="0"/>
                        <xsd:choice>
                            <xsd:element name="chemical" type="tml:chemicalRefType">
                                <xsd:annotation>
                                    <xsd:documentation>link to single component</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="system" type="tml:systemRefType">
                                <xsd:annotation>
                                    <xsd:documentation>link to prior samples, components, or processing ( characterized components/samples)</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:choice minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>choice of representation of composition amount as a single value or a tolerance range.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:element name="value" type="tml:numberOrNullType" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>amount of component</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="tolerance" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>values given as a
                                        tolerance</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence minOccurs="1" maxOccurs="1">
                                        <xsd:element name="low" type="tml:numberOrNullType"
                                            minOccurs="1">
                                            <xsd:annotation>
                                                <xsd:documentation>limit of composition
                                                  tolerance</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="high" type="tml:numberOrNullType"
                                            minOccurs="1">
                                            <xsd:annotation>
                                                <xsd:documentation>upper limit of composition
                                                  tolerance</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:element default="unstated percent" name="units" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>units to describe amount of component
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="mass percent"/>
                                    <xsd:enumeration value="mass percent halide impurity"/>
                                    <xsd:enumeration value="mass percent water"/>
                                    <xsd:enumeration value="mole percent"/>
                                    <xsd:enumeration value="mole percent halide impurity"/>
                                    <xsd:enumeration value="mole percent water"/>
                                    <xsd:enumeration value="volume percent"/>
                                    <xsd:enumeration value="unstated percent"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="multiComponentType">
            <xsd:annotation>
                <xsd:documentation>identification of type of multicomponent system: alloy, clathrate, complex, crystal, solution</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="alloy"/>
                    <xsd:enumeration value="clathrate"/>
                    <xsd:enumeration value="complex"/>
                    <xsd:enumeration value="crystal"/>
                    <xsd:enumeration value="solution"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="compositionBasis">
            <xsd:annotation>
                <xsd:documentation>identification of composition bases (# molecules, mole or weight fraction)</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="number of molecules"/>
                    <xsd:enumeration value="mole fraction"/>
                    <xsd:enumeration value="mass fraction"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="processType">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:annotation>
                <xsd:documentation>to identify (1) the specimen/chemical that processing was done on.  and (2) the processing steps conducted on the specimen/chemical. Listed in order of action.</xsd:documentation>
            </xsd:annotation>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:choice>
                <xsd:annotation>
                    <xsd:documentation>choice to identify chemical or system(specimen) processing was done on.</xsd:documentation>
                </xsd:annotation>
                <xsd:element name="chemical" type="tml:chemicalRefType">
                    <xsd:annotation>
                        <xsd:documentation>link for single uncharacterized component that is to be processed</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="system" type="tml:systemRefType">
                    <xsd:annotation>
                        <xsd:documentation>link to prior samples, components.
or processing (of characterized components/samples)</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element maxOccurs="unbounded" name="processingStep">
                <xsd:annotation>
                    <xsd:documentation>Material specimen processing prior to measurement: includes any
                special preparation processes, thermal processing, mechanical processing, and
                purification methods.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence maxOccurs="1">
                        <xsd:choice>
                            <xsd:element name="heatingCooling">
                                <xsd:annotation>
                                    <xsd:documentation>Heating/Cooling</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:choice minOccurs="0">
                                        <xsd:element name="cooling">
                                            <xsd:annotation>
                                                <xsd:documentation>Cooling</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Air"/>
                                                  <xsd:enumeration value="Casting"/>
                                                  <xsd:enumeration value="Furnace"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="modifiedSolidification">
                                            <xsd:annotation>
                                                <xsd:documentation>Modified Solidification</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Seeded Solidification"/>
                                                  <xsd:enumeration
                                                  value="Directional Solidification"/>
                                                  <xsd:enumeration
                                                  value="Single Crystal Solidification"/>
                                                  <xsd:enumeration value="Zone Refining"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="quenching">
                                            <xsd:annotation>
                                                <xsd:documentation>Quenching</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Ice Quenching"/>
                                                  <xsd:enumeration value="Liquid Nitrogen Quenching"/>
                                                  <xsd:enumeration value="Oil Quenching"/>
                                                  <xsd:enumeration value="Water Quenching"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="rapidCooling">
                                            <xsd:annotation>
                                                <xsd:documentation>Rapid Cooling</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Atomization Processing"/>
                                                  <xsd:enumeration value="Melt Spinning"/>
                                                  <xsd:enumeration value="Splat Cooling"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="rapidHeating">
                                            <xsd:annotation>
                                                <xsd:documentation>Rapid Heating</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Electron Beam Processing"/>
                                                  <xsd:enumeration value="Laser Processing"/>
                                                  <xsd:enumeration value="Microwave Processing"/>
                                                  <xsd:enumeration value="Resistive Heating"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="heating">
                                            <xsd:annotation>
                                                <xsd:documentation>Heating</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="mechanicalProcessing">
                                <xsd:annotation>
                                    <xsd:documentation>Mechanical Processing</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:choice minOccurs="0">
                                        <xsd:element name="acousticWave">
                                            <xsd:annotation>
                                                <xsd:documentation>Acoustic Wave</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Shock Wave"/>
                                                  <xsd:enumeration value="Ultrasonic"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="deforming">
                                            <xsd:annotation>
                                                <xsd:documentation>Deforming</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Extruding"/>
                                                  <xsd:enumeration value="Forming"/>
                                                  <xsd:enumeration value="Rolling"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="machining">
                                            <xsd:annotation>
                                                <xsd:documentation>Machining</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Machining Surface"/>
                                                  <xsd:enumeration value="Machining to Shape"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="millingGrinding">
                                            <xsd:annotation>
                                                <xsd:documentation>Milling/Grinding</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Ball milling"/>
                                                  <xsd:enumeration value="Filing"/>
                                                  <xsd:enumeration value="Mortar and pestle"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="pressing">
                                            <xsd:annotation>
                                                <xsd:documentation>Pressing</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Bonding"/>
                                                  <xsd:enumeration value="Die Pressing"/>
                                                  <xsd:enumeration value="Isostatic Pressing"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="preparation">
                                <xsd:annotation>
                                    <xsd:documentation>Preparation</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:choice minOccurs="0">
                                        <xsd:element name="additiveManufacture">
                                            <xsd:annotation>
                                                <xsd:documentation>a process of joining materials to make objects from 3D model data, usually layer upon layer, as opposed to subtractive manufacturing methodologies.	a process of joining materials to make objects from 3D model data, usually layer upon layer, as opposed to subtractive manufacturing methodologies.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration
                                                  value="Directed Energy Deposit- E beam"/>
                                                  <xsd:enumeration value="Powder Bed Fusion- Laser"/>
                                                  <xsd:enumeration value="Powder Bed Fusion- E beam"/>
                                                  <xsd:enumeration
                                                  value="Directed Energy Deposition- Laser"/>
                                                  <xsd:enumeration value="Material Extrusion"/>
                                                  <xsd:enumeration value="Binder Jetting"/>
                                                  <xsd:enumeration value="Material Jetting"/>
                                                  <xsd:enumeration value="Sheet Lamination"/>
                                                  <xsd:enumeration value="Vat Photopolymerization"/>

                                                  <xsd:enumeration value=""/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="chemical">
                                            <xsd:annotation>
                                                <xsd:documentation>Chemical</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Cleaning"/>
                                                  <xsd:enumeration value="Etching"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="depositionProcessing">
                                            <xsd:annotation>
                                                <xsd:documentation>Deposition Processing</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Sputter Depositing"/>
                                                  <xsd:enumeration value="Chemical Vapor Depositing"/>
                                                  <xsd:enumeration value="Electrodeposition"/>
                                                  <xsd:enumeration value="Ion Beam Depositing"/>
                                                  <xsd:enumeration value="Physical Vapor Depositing"
                                                  />
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="meltingCasting">
                                            <xsd:annotation>
                                                <xsd:documentation>Melting &amp; Casting</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Arc Melting"/>
                                                  <xsd:enumeration value="Casting"/>
                                                  <xsd:enumeration value="Induction Melting"/>
                                                  <xsd:enumeration value="Radiation/Furnace Melting"/>
                                                  <xsd:enumeration value="Vacuum Degassing"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="modifiedSolidification">
                                            <xsd:annotation>
                                                <xsd:documentation>Modified Solidification</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Seeded Solidification"/>
                                                  <xsd:enumeration
                                                  value="Directional Solidification"/>
                                                  <xsd:enumeration
                                                  value="Single Crystal Solidification"/>
                                                  <xsd:enumeration value="Zone Refining"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="powder">
                                            <xsd:annotation>
                                                <xsd:documentation>preparation of a powder</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Atomization Processing"/>
                                                  <xsd:enumeration value="Chemical Reducing"/>
                                                  <xsd:enumeration value="Electrolysis  Processing"/>
                                                  <xsd:enumeration value="Mechanical Crushing"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="rapidHeating">
                                            <xsd:annotation>
                                                <xsd:documentation>Rapid Heating</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Electron Beam Processing"/>
                                                  <xsd:enumeration value="Laser Processing"/>
                                                  <xsd:enumeration value="Microwave Processing"/>
                                                  <xsd:enumeration value="Resistive Heating"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="purification">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="method">
                                            <xsd:simpleType>
                                                <xsd:annotation>
                                                  <xsd:documentation>Purification method(s) used to purify
                                            sample</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Impurity adsorption"/>
                                                  <xsd:enumeration value="Vacuum degasification"/>
                                                  <xsd:enumeration
                                                  value="Chemical reagent treatment"/>
                                                  <xsd:enumeration value="Crystallization from melt"/>
                                                  <xsd:enumeration
                                                  value="Crystallization from solution"/>
                                                  <xsd:enumeration value="Liquid chromatography"/>
                                                  <xsd:enumeration
                                                  value="Dried with chemical reagent"/>
                                                  <xsd:enumeration value="Dried in a desiccator"/>
                                                  <xsd:enumeration value="Dried by oven heating"/>
                                                  <xsd:enumeration value="Dried by vacuum heating"/>
                                                  <xsd:enumeration
                                                  value="De-gassed by boiling or ultrasonically"/>
                                                  <xsd:enumeration value="De-gassed by evacuation"/>
                                                  <xsd:enumeration
                                                  value="De-gassed by freezing and melting in vacuum"/>
                                                  <xsd:enumeration
                                                  value="Fractional crystallization"/>
                                                  <xsd:enumeration value="Fractional distillation"/>
                                                  <xsd:enumeration value="Molecular sieve treatment"/>
                                                  <xsd:enumeration value="Unspecified"/>
                                                  <xsd:enumeration
                                                  value="Preparative gas chromatography"/>
                                                  <xsd:enumeration value="Sublimation"/>
                                                  <xsd:enumeration value="Steam distillation"/>
                                                  <xsd:enumeration value="Solvent extraction"/>
                                                  <xsd:enumeration value="Salting out of solution"/>
                                                  <xsd:enumeration value="Zone refining"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="otherMethod" type="xsd:string"/>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="thermalProcessing">
                                <xsd:annotation>
                                    <xsd:documentation>Thermal Processing</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:choice minOccurs="0">
                                        <xsd:element name="annealing">
                                            <xsd:annotation>
                                                <xsd:documentation>Annealing</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Precipitation Annealing"/>
                                                  <xsd:enumeration value="Relaxation Annealing"/>
                                                  <xsd:enumeration value="Solution Annealing"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="melting">
                                            <xsd:annotation>
                                                <xsd:documentation>Melting</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Shock Wave"/>
                                                  <xsd:enumeration value="Ultrasonic"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element name="sintering">
                                            <xsd:annotation>
                                                <xsd:documentation>Sintering</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value=""/>
                                                  <xsd:enumeration value="Laser Sintering"/>
                                                  <xsd:enumeration
                                                  value="Radiation Sintering (Furnace)"/>
                                                  <xsd:enumeration value="Resistance Sintering"/>
                                                  <xsd:enumeration value="Spark Plasma Sintering"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:element name="temperature" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>temperature associated with processing step</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="info" type="tml:temperatureType"/>
                                    <xsd:element name="val" type="tml:valueType"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="pressure" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>pressure associated with processing step</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="info" type="tml:pressureType"/>
                                    <xsd:element name="val" type="tml:valueType"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="time" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>time associated with processing step</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="unit">
                                        <xsd:simpleType>
                                            <xsd:restriction base="xsd:string">
                                                <xsd:enumeration value="minutes"/>
                                                <xsd:enumeration value="hours"/>
                                                <xsd:enumeration value="days"/>
                                                <xsd:enumeration value="seconds"/>
                                            </xsd:restriction>
                                        </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="val" type="tml:valueType"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="rate" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>rate associated with processing step</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="unit">
                                        <xsd:simpleType>
                                            <xsd:restriction base="xsd:string">
                                                <xsd:enumeration value="K/s"/>
                                                <xsd:enumeration value="K/min"/>
                                                <xsd:enumeration value="K/hr"/>
                                            </xsd:restriction>
                                        </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="val" type="tml:valueType"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="crucibleMaterial" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>crucible material associated with processing step</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="Al2O3"/>
                                    <xsd:enumeration value="aluminum"/>
                                    <xsd:enumeration value="BeO"/>
                                    <xsd:enumeration value="BN"/>
                                    <xsd:enumeration value="brass"/>
                                    <xsd:enumeration value="copper"/>
                                    <xsd:enumeration value="graphite"/>
                                    <xsd:enumeration value="iron"/>
                                    <xsd:enumeration value="MgO"/>
                                    <xsd:enumeration value="molybdenum"/>
                                    <xsd:enumeration value="platinum"/>
                                    <xsd:enumeration value="Pt/Rh"/>
                                    <xsd:enumeration value="SiC"/>
                                    <xsd:enumeration value="Si3N4"/>
                                    <xsd:enumeration value="silica"/>
                                    <xsd:enumeration value="silver"/>
                                    <xsd:enumeration value="spinel"/>
                                    <xsd:enumeration value="stainless_steel"/>
                                    <xsd:enumeration value="tantalum"/>
                                    <xsd:enumeration value="ThO2"/>
                                    <xsd:enumeration value="TiO2"/>
                                    <xsd:enumeration value="tungsten"/>
                                    <xsd:enumeration value="W/Re"/>
                                    <xsd:enumeration value="Y2O3"/>
                                    <xsd:enumeration value="YSZ"/>
                                    <xsd:enumeration value="zinc"/>
                                    <xsd:enumeration value="ZrO2"/>
                                    <xsd:enumeration value="ZrO2/Zr"/>
                                    <xsd:enumeration value="None"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="percentReduction" type="xsd:float" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation> percent reduction associated with processing step (positive numbers indicates a reduction)</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="atmosphere" minOccurs="0">
                            <xsd:complexType>
                                <xsd:choice>
                                    <xsd:element name="atmosphere">
                                        <xsd:simpleType>
                                            <xsd:restriction base="xsd:string">
                                                <xsd:enumeration value="vacuum"/>
                                                <xsd:enumeration value="air"/>
                                                <xsd:enumeration value="nitrogen"/>
                                                <xsd:enumeration value="argon"/>
                                                <xsd:enumeration value="oxygen"/>
                                                <xsd:enumeration value="helium"/>
                                                <xsd:enumeration value="hydrogen"/>
                                                <xsd:enumeration value="Ar+H2"/>
                                                <xsd:enumeration value="Ar+He"/>
                                                <xsd:enumeration value="Ar+CO2"/>
                                                <xsd:enumeration value="Ar+N"/>
                                                <xsd:enumeration value="He+N"/>
                                                <xsd:enumeration value="CO2"/>
                                                <xsd:enumeration value="Ammonia"/>
                                                <xsd:enumeration value="Air"/>
                                                <xsd:enumeration value="oil"/>
                                                <xsd:enumeration value="kerosene"/>
                                                <xsd:enumeration value="salts"/>
                                            </xsd:restriction>
                                        </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="other">
                                        <xsd:simpleType>
                                            <xsd:restriction base="xsd:string"/>
                                        </xsd:simpleType>
                                    </xsd:element>
                                </xsd:choice>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="sampleType">
        <xsd:annotation>
            <xsd:documentation>sample</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:choice maxOccurs="unbounded">
                <xsd:element name="chemical" type="tml:chemicalRefType">
                    <xsd:annotation>
                        <xsd:documentation>link for single uncharacterized component of sample</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="system" type="tml:systemRefType">
                    <xsd:annotation>
                        <xsd:documentation>link to prior samples, components, or processing (of characterized components/samples)</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="source" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>source of material (i.e. Alfa Aesar, Goodfellow,
                        etc.)</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:annotation>
                        <xsd:documentation>Source(s) of the sample</xsd:documentation>
                    </xsd:annotation>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Commercial source"/>
                        <xsd:enumeration value="Synthesized by the authors"/>
                        <xsd:enumeration value="Synthesized by others"/>
                        <xsd:enumeration value="Standard Reference Material (SRM)"/>
                        <xsd:enumeration value="Isolated from a natural product"/>
                        <xsd:enumeration value="Not stated in the document"/>
                        <xsd:enumeration value="No sample used"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="reportingStatus" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>information pertaining to the sample (not described, unknown,
                        from previous paper)</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:annotation>
                        <xsd:documentation/>
                    </xsd:annotation>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="unknown"/>
                        <xsd:enumeration value="notDescribed"/>
                        <xsd:enumeration value="previousPaper"/>
                        <xsd:enumeration value="noSample"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="componentCharacterization" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Component information of the sample from impurity
                        information</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="label" type="xsd:string" minOccurs="0"/>
                        <xsd:choice>
                            <xsd:annotation>
                                <xsd:documentation>known chemical/ component/
                                    impurity</xsd:documentation>
                            </xsd:annotation>
                            <xsd:element name="chemical" minOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation>known chemical/component
                                        impurity</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="ref" type="xsd:positiveInteger">
                                        <xsd:annotation>
                                            <xsd:documentation>reference to chemical id# for cross reference purposes.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="unknown" minOccurs="1" type="xsd:boolean">
                                <xsd:annotation>
                                    <xsd:documentation>unknown (i.e. specimen only defined 95% pure,
                                        therefore 5% is unknown) </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="1" name="class">
                                <xsd:annotation>
                                    <xsd:documentation>other classes of elements e.g. halide,
                                        etc.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="halide"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="otherClass" type="xsd:string" minOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation>used when generic "halide impurities" are
                                        quantified rather than specific halides.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:choice minOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation>amount of component (purity, chemical
                                    composition range, etc.)</xsd:documentation>
                            </xsd:annotation>
                            <xsd:element name="value" type="tml:numberOrNullType" minOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation>amount of component</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:sequence minOccurs="1">
                                <xsd:element name="low" type="tml:numberOrNullType" minOccurs="1">
                                    <xsd:annotation>
                                        <xsd:documentation>lower limit of
                                            tolerance</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                                <xsd:element name="high" type="tml:numberOrNullType" minOccurs="1">
                                    <xsd:annotation>
                                        <xsd:documentation>upper limit of
                                            tolerance</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:choice>
                        <xsd:element default="unstated percent" name="units">
                            <xsd:annotation>
                                <xsd:documentation>units to describe amount of impurity
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="mass percent"/>
                                    <xsd:enumeration value="mass percent halide impurity"/>
                                    <xsd:enumeration value="mass percent water"/>
                                    <xsd:enumeration value="mole percent"/>
                                    <xsd:enumeration value="mole percent halide impurity"/>
                                    <xsd:enumeration value="mole percent water"/>
                                    <xsd:enumeration value="volume percent"/>
                                    <xsd:enumeration value="unstated percent"/>
                                    <xsd:enumeration value="a"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:choice>
                            <xsd:element name="analyticalMethod" minOccurs="1" maxOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation>Analytical method used to quantify
                                        impurities</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:annotation>
                                        <xsd:documentation>Analytical method used to measure
                                            purity.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="Chemical analysis"/>
                                        <xsd:enumeration
                                            value="Difference between bubble and dew temperatures"/>
                                        <xsd:enumeration value="Density"/>
                                        <xsd:enumeration value="DSC"/>
                                        <xsd:enumeration value="Estimation"/>
                                        <xsd:enumeration value="Gas chromatography"/>
                                        <xsd:enumeration
                                            value="Fraction melting in an adiabatic calorimeter"/>
                                        <xsd:enumeration value="Mass spectrometry"/>
                                        <xsd:enumeration value="NMR (proton)"/>
                                        <xsd:enumeration value="NMR (other)"/>
                                        <xsd:enumeration value="Not known"/>
                                        <xsd:enumeration value="Spectroscopy"/>
                                        <xsd:enumeration
                                            value="Thermal analysis using temperature-time measurement"/>
                                        <xsd:enumeration value="Acid-base titration"/>
                                        <xsd:enumeration value="Other types of titration"/>
                                        <xsd:enumeration value="Mass loss on drying"/>
                                        <xsd:enumeration value="Karl Fischer titration"/>
                                        <xsd:enumeration value="HPLC"/>
                                        <xsd:enumeration value="Ion chromatography"/>
                                        <xsd:enumeration value="Ion-selective electrode"/>
                                        <xsd:enumeration value="CO2 yield in combustion products"/>
                                        <xsd:enumeration value="Other"/>
                                        <xsd:enumeration value="Estimated by the compiler"/>
                                        <xsd:enumeration value="Stated by supplier"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="otherAnalyticalMethod" type="xsd:string"
                                minOccurs="1" maxOccurs="1"/>
                        </xsd:choice>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="materialForm">
                <xsd:annotation>
                    <xsd:documentation>physical description of specimen (for metals and
                        ceramics)</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="materialForm" minOccurs="1" maxOccurs="1"
                            default="unknown">
                            <xsd:annotation>
                                <xsd:documentation>description of form of material (sheet, powder,
                                    foil, bar, etc.)</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="unknown"/>
                                    <xsd:enumeration value="Liquid"/>
                                    <xsd:enumeration value="Foil"/>
                                    <xsd:enumeration value="Sheet"/>
                                    <xsd:enumeration value="Plate"/>
                                    <xsd:enumeration value="Powder"/>
                                    <xsd:enumeration value="Ribbon"/>
                                    <xsd:enumeration value="Wire"/>
                                    <xsd:enumeration value="Rod"/>
                                    <xsd:enumeration value="Bar"/>
                                    <xsd:enumeration value="Tube"/>
                                    <xsd:enumeration value="not stated"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element default="0" minOccurs="0" name="singleCrystal"
                            type="xsd:boolean"/>
                        <xsd:element name="microstructuralFeatures" maxOccurs="unbounded"
                            minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>grain size as measured, if two selected allows
                                    for bimodal values to be captured. ref is for
                                    phase.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="label" type="xsd:string" minOccurs="0"/>
                                    <xsd:element name="phase" minOccurs="0" maxOccurs="1"
                                        type="tml:phaseRefType">
                                        <xsd:annotation>
                                            <xsd:documentation>link to phase(s) of material for which the microstructural feature describes</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice minOccurs="0">
                                        <xsd:element default="unknown" minOccurs="0"
                                            name="grainMorphology">
                                            <xsd:annotation>
                                                <xsd:documentation>grain morphology--not a complex
                                                description. (i.e. elongated grains, bimodal gs
                                                distribution, unknown, equiaxed, equiaxed)
                                            </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="equiaxed grains"/>
                                                  <xsd:enumeration value="lamellar grains "/>
                                                  <xsd:enumeration value="chinese script grains"/>
                                                  <xsd:enumeration value="elongated/pancake grains"/>
                                                  <xsd:enumeration value="unknown"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element minOccurs="0" name="otherMorphology"
                                            type="xsd:string"/>
                                    </xsd:choice>
                                    <xsd:choice minOccurs="0">
                                        <xsd:element default="unknown" minOccurs="0" name="texture">
                                            <xsd:annotation>
                                                <xsd:documentation>restricted list of well known textures</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                                <xsd:restriction base="xsd:string">
                                                  <xsd:enumeration value="Goss"/>
                                                  <xsd:enumeration value="Cube"/>
                                                  <xsd:enumeration value="Brass"/>
                                                  <xsd:enumeration value="Copper"/>
                                                  <xsd:enumeration value="unknown"/>
                                                </xsd:restriction>
                                            </xsd:simpleType>
                                        </xsd:element>
                                        <xsd:element minOccurs="0" name="otherTexture"
                                            type="xsd:string"/>
                                    </xsd:choice>
                                    <xsd:element minOccurs="0" name="grainSize"
                                        type="tml:microstructuralEvalType">
                                        <xsd:annotation>
                                            <xsd:documentation>measured grain size</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="aspectRatio"
                                        type="tml:microstructuralEvalType">
                                        <xsd:annotation>
                                            <xsd:documentation>in case of elongated grains, the
                                                aspect ratio can be captured</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="reactionType">
        <!-- This needs to be populated -->
        <xsd:sequence>
            <xsd:element minOccurs="0" name="label" type="xsd:string"/>
            <xsd:element name="participant" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>participating elements/compounds in
                        reaction</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="label" type="xsd:string"/>
                        <xsd:choice>
                            <xsd:element name="chemical" type="tml:chemicalRefType">
                                <xsd:annotation>
                                    <xsd:documentation>link for common identification of participant
                                        to a known chemical species</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="system" type="tml:systemRefType">
                                <xsd:annotation>
                                    <xsd:documentation>link for common identification of components
                                        in the system</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:element name="stoichiometricCoef" type="xsd:float" minOccurs="0"/>
                        <xsd:element name="phase" minOccurs="0" type="tml:phaseRefType">
                            <xsd:annotation>
                                <xsd:documentation>link to phase of participant chemical/compounds</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="solvent" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>solution in which the participates are in</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="label" type="xsd:string"/>
                        <xsd:choice>
                            <xsd:element name="chemical" type="tml:chemicalRefType">
                                <xsd:annotation>
                                    <xsd:documentation>link for common identification of participant
                                        to a known chemical species</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="system" type="tml:systemRefType">
                                <xsd:annotation>
                                    <xsd:documentation>link for common identification of components
                                        in the system</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:element name="phase" minOccurs="0" type="tml:phaseRefType">
                            <xsd:annotation>
                                <xsd:documentation>link to phase of solvent</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="auxiliarySubstance" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>substances that do not participate in the reaction that are present</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="label" type="xsd:string"/>
                        <xsd:choice>
                            <xsd:element name="chemical" type="tml:chemicalRefType">
                                <xsd:annotation>
                                    <xsd:documentation>link for common identification of participant
                                        to a known chemical species</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="system" type="tml:systemRefType">
                                <xsd:annotation>
                                    <xsd:documentation>link for common identification of components
                                        in the system</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:choice minOccurs="0">
                            <xsd:element name="function">
                                <xsd:annotation>
                                    <xsd:documentation>Purpose of auxillary substance (cofactor,
                                        buffer, catalyst, etc.)</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="cofactor"/>
                                        <xsd:enumeration value="buffer"/>
                                        <xsd:enumeration value="inert"/>
                                        <xsd:enumeration value="catalyst"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="otherFunction" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation>string for identification of function of
                                        substance if not listed in standard
                                        functions.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:element name="phase" minOccurs="0" type="tml:phaseRefType">
                            <xsd:annotation>
                                <xsd:documentation>link to phase of auxiliary substance</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="electronNumber" type="xsd:integer" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>electron number associated with reaction</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="reactionFormalism">
                <xsd:annotation>
                    <xsd:documentation>If the chemical formalism is used, thermodynamic
                        equilibrium constants depend on temperature only, and apparent
                        equilibrium constants (i.e., those expressed in terms of concentration) further depend on activity coefficients of the species participating in the reaction. In the biochemical formalism, thermodynamic equilibrium constants cannot be defined, and apparent equilibrium constants expressed through total concentration of species in various forms of dissociation and complexation depend on many factors, such as pH, pMg, ionic strength, and so forth. The number of these factors is not strictly determined.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="chemical"/>
                        <xsd:enumeration value="biochemical"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="reactionType">
                <xsd:annotation>
                    <xsd:documentation>selection of reaction type from list</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration
                            value="addition of various compounds to unsaturated compounds"/>
                        <xsd:enumeration
                            value="addition of water to a liquid or solid to produce a hydrate"/>
                        <xsd:enumeration value="atomization (or formation from atoms)"/>
                        <xsd:enumeration value="combustion with oxygen"/>
                        <xsd:enumeration value="combustion with other elements or compounds"/>
                        <xsd:enumeration value="esterification"/>
                        <xsd:enumeration value="exchange of alkyl groups"/>
                        <xsd:enumeration value="exchange of hydrogen (atoms) with other groups"/>
                        <xsd:enumeration
                            value="Formation of a compound from elements in their stable state"/>
                        <xsd:enumeration value="formation of ion"/>
                        <xsd:enumeration
                            value="halogenation (addition of or replacement by a halogen)"/>
                        <xsd:enumeration
                            value="hydrogenation (addition of hydrogen to unsaturated compounds)"/>
                        <xsd:enumeration value="homonuclear dimerization"/>
                        <xsd:enumeration value="hydrohalogenation"/>
                        <xsd:enumeration value="hydrolysis of ions"/>
                        <xsd:enumeration value="other reaction with water"/>
                        <xsd:enumeration value="ion exchange"/>
                        <xsd:enumeration value="neutralization (reaction of an acid with a base)"/>
                        <xsd:enumeration value="oxidation with oxidizing agents other than oxygen"/>
                        <xsd:enumeration value="oxidation with oxygen (not complete)"/>
                        <xsd:enumeration value="polymerization (all other types)"/>
                        <xsd:enumeration value="solvolyis (solvents other than water)"/>
                        <xsd:enumeration value="stereoisomerism"/>
                        <xsd:enumeration value="structural isomerization"/>
                        <xsd:enumeration value="other reaction"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="RInChI" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>reaction definition according to the IUPAC International
                        Chemical Identifier</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="reactionSMILES" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>reaction definition according to SMILES
                        standards</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="sybylLineNotation" minOccurs="0" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>reaction definition according to Sybyl Line
                        Notation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

