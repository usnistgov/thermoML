    </xsd:complexType>
    <xsd:complexType name="massDensityType">
        <xsd:annotation>
            <xsd:documentation>used for critical density, mass density.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="kg/m^3">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="kg/m^3"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>method used for measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Burnett expansion method"/>
                            <xsd:enumeration value="Bellows volumetry"/>
                            <xsd:enumeration
                                value="Buoyancy - magnetic float or magnetic suspension"/>
                            <xsd:enumeration
                                value="Buoyancy - hydrostatic balance with magnetic float"/>
                            <xsd:enumeration
                                value="Buoyancy - hydrostatic balance with magnetic suspension - one sinker"/>
                            <xsd:enumeration
                                value="Buoyancy - hydrostatic balance with magnetic suspension - two sinkers"/>
                            <xsd:enumeration value="Calculated with densities of this investigation"/>
                            <xsd:enumeration
                                value="Calculated with a solvent density reported elsewhere"/>
                            <xsd:enumeration value="Density floats method"/>
                            <xsd:enumeration value="Derived analytically"/>
                            <xsd:enumeration
                                value="Derived from from index of refraction callibration"/>
                            <xsd:enumeration
                                value="Derived from shadow graph volume and levitated droplet mass"/>
                            <xsd:enumeration value="Derived from speed of sound"/>
                            <xsd:enumeration value="Derived graphically"/>
                            <xsd:enumeration value="Direct dilatometry"/>
                            <xsd:enumeration value="Hydrometer"/>
                            <xsd:enumeration value="Isochoric PVT measurement"/>
                            <xsd:enumeration value="Laser pulse heating"/>
                            <xsd:enumeration value="Other PVT measurement"/>
                            <xsd:enumeration value="Piezometry"/>
                            <xsd:enumeration value="Piezometry, constant-volume"/>
                            <xsd:enumeration value="Pycnometric method"/>
                            <xsd:enumeration value="Resistive pulse heating"/>
                            <xsd:enumeration value="Vibrating tube method"/>
                            <xsd:enumeration value="X-ray diffraction"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of method that is not listed in ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="molalityType">
        <xsd:annotation>
            <xsd:documentation>used in molality, ratio of amount of solute to mass of
                solution</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="mol/kg">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="mol/kg"/>
                        <!--unsure-->
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="solute" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>Solute component linked to property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" name="solvent" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>solvent component(s) linked to property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="By X=Y"/>
                            <xsd:enumeration value="By T or P extreme"/>
                            <xsd:enumeration value="Chromatography"/>
                            <xsd:enumeration value="Spectrophotometry"/>
                            <xsd:enumeration value="Density calibration data"/>
                            <xsd:enumeration value="Index of refraction calibration data"/>
                            <xsd:enumeration value="Speed of sound calibration data"/>
                            <xsd:enumeration value="Calculated by Gibbs-Duhem equation"/>
                            <xsd:enumeration value="Titration method"/>
                            <xsd:enumeration value="Phase equilibration"/>
                            <xsd:enumeration value="Derived from phase diagram analysis"/>
                            <xsd:enumeration value="Photoacoustic method"/>
                            <xsd:enumeration value="Other"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="phaseVolumeFractionType">
        <xsd:annotation>
            <xsd:documentation>used for phase volume fraction</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="phase" minOccurs="0" type="tml:phaseRefType">
                <xsd:annotation>
                    <xsd:documentation>link to phase associated with property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>analytical instrument used for measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Statistical analysis of micrographs"/>
                            <xsd:enumeration value="iterative polishing/micrograph reconstruction"/>
                            <xsd:enumeration value="X-ray diffraction"/>
                            <xsd:enumeration value="x-ray tomography"/>
                            <xsd:enumeration value="iterative EBSD/polishing reconstruction"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a analytical instrument that is not listed in ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="pressureType">
        <xsd:annotation>
            <xsd:documentation>used for critical pressure, lower consolute pressure, upper
                consolute pressure, cryoscopic constant molality type, phase boundary pressure,
                triple point pressure, azeotrope pressure, Vapor pressure, Henry's law constant mole
                fraction scale, osmotic pressure, </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="Pa">
                <xsd:annotation>
                    <xsd:documentation>measured unit of variable/property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Pa"/>
                        <xsd:enumeration value="mmHg"/>
                        <xsd:enumeration value="F"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>method used for measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Manometer"/>
                            <xsd:enumeration value="hydrostatic guage"/>
                            <xsd:enumeration value="Aneroid gage"/>
                            <xsd:enumeration value="Thermocouple gage"/>
                            <xsd:enumeration value="Ionization gage"/>
                            <xsd:enumeration value="Hot cathode"/>
                            <xsd:enumeration value="Cold cathode"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element minOccurs="0" name="partial">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="component"
                            type="tml:chemicalRefType" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>solvent component(s) linked to property</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="volumeFractionType">
        <xsd:annotation>
            <xsd:documentation>used for eutectic composition (vol frac.), lower consolute
                composition (vol frac.), upper consolute composition (vol frac.), volume fraction,
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string"> </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="component" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>component linked to property </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="phase" type="tml:phaseRefType"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- *****************************************-->
