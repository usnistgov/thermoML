                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="integrating (Ulbricht) sphere"/>
                            <xsd:enumeration value="ellipsometry"/>
                            <xsd:enumeration value="radiance comparison"/>
                            <xsd:enumeration value="division of amplitude photopolarimeter"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="totalNormalEmittanceType">
        <!--need to add wavelength-->
        <xsd:annotation>
            <xsd:documentation>used for total normal emittance</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="division of amplitude photopolarimeter"/>
                            <xsd:enumeration value="ellipsometry"/>
                            <xsd:enumeration value="radiance comparison"/>
                            <xsd:enumeration value="integrating (Ulbricht) sphere"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="transitionTemperatureType">
        <xsd:annotation>
            <xsd:documentation>for all phase transition temperatures. Requires listing of
                involved phases, and where applicable identified phases with phase fractions
                approaching zero </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="K">
                <xsd:annotation>
                    <xsd:documentation>measured unit of variable/property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="K"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="scale" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>temperature scale</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="ITS-90"/>
                        <xsd:enumeration value="EPT-76"/>
                        <xsd:enumeration value="IPTS-68"/>
                        <xsd:enumeration value="IPTS-48"/>
                        <xsd:enumeration value="ITS-27"/>
                        <xsd:enumeration value="NBS-55"/>
                        <xsd:enumeration value="He3-62"/>
                        <xsd:enumeration value="He4-62"/>
                        <xsd:enumeration value="He4-58"/>
                        <xsd:enumeration value="He4-55E"/>
                        <xsd:enumeration value="He4-49"/>
                        <xsd:enumeration value="thermodynamic"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="phaseTransitionDetails" type="tml:phaseTransitionType"/>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="determiend by radiance (radiance temperature)"/>
                            <xsd:enumeration value="platinum resistance thermometer"/>
                            <xsd:enumeration value="pyrometer"/>
                            <xsd:enumeration value="pyrometer, optical"/>
                            <xsd:enumeration value="pyrometer, infrared"/>
                            <xsd:enumeration value="pyrometer, disappearing filament"/>
                            <xsd:enumeration value="thermistor"/>
                            <xsd:enumeration value="thermocouple"/>
                            <xsd:enumeration value="thermocouple J -Fe/Constantan"/>
                            <xsd:enumeration value="thermocouple K -Chromel/Alumel"/>
                            <xsd:enumeration value="thermocouple E -Chromel/Constantan"/>
                            <xsd:enumeration value="thermocouple T -Cu/Constantin"/>
                            <xsd:enumeration value="thermocouple N -Nicrosil/Nisil"/>
                            <xsd:enumeration value="thermocouple B -Pt/Pt-30%Rh"/>
                            <xsd:enumeration value="thermocouple S -Pt/Pt-10%Rh"/>
                            <xsd:enumeration value="thermocouple R -Pt/Pt-13%Rh"/>
                            <xsd:enumeration value="thermocouple U -Cu/Cu-Ni"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="transportNumberType">
        <!--nomethods-->
        <xsd:annotation>
            <xsd:documentation>used for transport number</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="moving boundary"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="ion" type="tml:chemicalRefType"/>
            <xsd:element name="otherIons" type="tml:chemicalRefType" maxOccurs="unbounded"
                minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="viscosityType">
        <xsd:annotation>
            <xsd:documentation>used for viscosity, excess viscosity, </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="Pa s">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Pa s"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="capillary tube (Ostwald, Ubbelohde) method"/>
                            <xsd:enumeration value="concentric cylinder viscometery (vertical)"/>
                            <xsd:enumeration value="cone and plate viscometery"/>
                            <xsd:enumeration value="dampened torsional vibration methods"/>
                            <xsd:enumeration value="draining crucible"/>
                            <xsd:enumeration value="falling cup viscometer"/>
                            <xsd:enumeration value="falling or rolling sphere viscometery"/>
                            <xsd:enumeration value="falling  or alternating body method viscometer"/>
                            <xsd:enumeration value="moving piston method"/>
                            <xsd:enumeration value="oscillating cup viscometery"/>
                            <xsd:enumeration value="oscillating disc viscometery"/>
                            <xsd:enumeration value="oscillating droplet dampening method"/>
                            <xsd:enumeration value="oscillating plate viscometery"/>
                            <xsd:enumeration value="oscillating sphere viscometery"/>
                            <xsd:enumeration
                                value="oscillating vessel viscometer (any other geometries)"/>
                            <xsd:enumeration value="surface laser light scattering (SLLS)"/>
                            <xsd:enumeration value="vibrating wire viscometery"/>
                            <xsd:enumeration
                                value="stabinger viscometer (horizontal concentric cylinder)"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="volumeRatioOfSoluteToSolventType">
        <xsd:annotation>
            <xsd:documentation>used in volume ratio of solute to solvent</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="solute" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>Solute component linked to property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" name="solvent" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>solvent component(s) linked to property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string"> </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="waveLengthType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:double" minOccurs="0"/>
            <xsd:element name="units" default="m">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="m"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="metaData">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="instrumentMetaData" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>field for description of instrument producing wavelength and any characterization of wavelength</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="waveLengthImage" type="xsd:anyURI">
                            <xsd:annotation>
                                <xsd:documentation>any link to the characterized wavelength measurement spectrum graphs, or other content.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="waveLengthDescription" use="required">
            <xsd:annotation>
                <xsd:documentation>wavelength description</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="gaussian maximum wavelength"/>
                    <xsd:enumeration value="full Width at Half Maximum"/>
                    <xsd:enumeration value="low break wavelength"/>
                    <xsd:enumeration value="high break wavelength"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="otherPropertyType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" type="xsd:string"/>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

    <!--***********************************************-->
    <!--         key-key reference definitions         -->
    <!--***********************************************-->

    <xsd:complexType name="chemicalRefType">
        <xsd:attribute name="ref" type="xsd:positiveInteger">
            <xsd:annotation>
                <xsd:documentation>reference to chemical id# for cross reference purposes.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="citationRefType">
        <xsd:attribute name="ref" type="xsd:positiveInteger">
            <xsd:annotation>
                <xsd:documentation>reference to citation id# for cross reference purposes.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="phaseRefType">
        <xsd:attribute name="ref" type="xsd:positiveInteger" use="required">
            <xsd:annotation>
                <xsd:documentation>reference to phase id# for cross reference purposes.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="systemRefType">
        <xsd:attribute name="ref" type="xsd:positiveInteger">
            <xsd:annotation>
                <xsd:documentation>reference to system id# for cross reference purposes.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="phaseFieldRefType">
        <xsd:attribute name="ref" type="xsd:positiveInteger">
            <xsd:annotation>
                <xsd:documentation>reference to phaseField id# (phases present) for cross reference purposes.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="refDescriptionRefType">
        <xsd:attribute name="ref" type="xsd:positiveInteger">
            <xsd:annotation>
                <xsd:documentation>reference to referenceDetails id#.
For cross reference purposes. Only useful for within this XML, non-transferable between XML documents.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="conditionRefType">
        <xsd:attribute name="ref" type="xsd:positiveInteger">
            <xsd:annotation>
                <xsd:documentation>reference of condition represented by a data point.  Reference to condition id#.
                    For cross reference purposes. Only useful for within this XML, non-transferable between XML documents.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="propRefType">
        <xsd:attribute name="ref" type="xsd:positiveInteger">
            <xsd:annotation>
                <xsd:documentation>reference of condition represented by a data point.  Reference to condition id#.
                    For cross reference purposes. Only useful for within this XML, non-transferable between XML documents.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>


    <!--*****************************************-->
    <!--         Null/number definitions         -->
    <!--*****************************************-->

    <!--used for instances where either a number or a nonvalid numeric value is necessary-->
    <xsd:simpleType name="numberOrNullType">
        <xsd:union memberTypes="xsd:double tml:nullEnumerationType"> </xsd:union>
    </xsd:simpleType>
    <!--used for instances where either a number or a nonvalid numeric value is necessary-->
    <xsd:simpleType name="nullEnumerationType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="outside detection limit"/>
            <xsd:enumeration value="undefined"/>
            <xsd:enumeration value="not measured"/>
            <xsd:enumeration value="unknown"/>
        </xsd:restriction>
    </xsd:simpleType>
    <!--used to describe the partial molar volume measurement-->
    <xsd:complexType name="partialMolarVolumeType">
        <!--unsure about some methods-->
        <xsd:annotation>
            <xsd:documentation>type for: partial molar volume </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="m^3/mol">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="m^3/mol"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="component" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>link to component associated with property </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="bellows volumetry"/>
                            <xsd:enumeration value="buoyancy - hydrostatic balance"/>
                            <xsd:enumeration
                                value="buoyancy - hydrostatic balance with magnetic float"/>
                            <xsd:enumeration
                                value="buoyancy - hydrostatic balance with magnetic suspension - one sinker"/>
                            <xsd:enumeration
                                value="buoyancy - hydrostatic balance with magnetic suspension - two sinkers"/>
                            <xsd:enumeration
                                value="buoyancy - magnetic float or magnetic suspension"/>
                            <xsd:enumeration value="burnett expansion method"/>
                            <xsd:enumeration value="constant-volume piezometry"/>
                            <xsd:enumeration value="derived analytically"/>
                            <xsd:enumeration value="derived from speed of sound"/>
                            <xsd:enumeration value="derived graphically"/>
                            <xsd:enumeration value="direct dilatometry"/>
                            <xsd:enumeration value="calculated with densities of this investigation"/>
                            <xsd:enumeration
                                value="calculated with a solvent density reported elsewhere"/>
                            <xsd:enumeration value="hydrostatic balance"/>
                            <xsd:enumeration value="isochoric PVT measurement"/>
                            <xsd:enumeration value="laser pulse heating"/>
                            <xsd:enumeration value="levitation methods"/>
                            <xsd:enumeration value="other PVT measurement"/>
                            <xsd:enumeration value="pycnometric method"/>
                            <xsd:enumeration value="resistive pulse heating"/>
                            <xsd:enumeration value="vibrating tube method"/>
                            <xsd:enumeration value="x-ray diffraction"/>
                            <xsd:enumeration value="shadow graph method"/>
                            <xsd:enumeration value="vibrating plate densiometer"/>
                            <xsd:enumeration value="vibrating wire densiometer"/>
                            <xsd:enumeration value="maximum bubble pressure"/>
                            <xsd:enumeration value="sessile drop method"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <!--used for the description of a frequency used in a measurement-->
    <xsd:complexType name="frequencyType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:double" minOccurs="0"/>
            <xsd:element name="units" default="Hz">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Hz"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string"> </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <!--used for the description of a voltage measured for an electrochemical cell-->
    <xsd:complexType name="emfType">
        <xsd:annotation>
            <xsd:documentation>Note: reaction information for cell is found in system
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="label" type="xsd:string"/>
            <xsd:choice maxOccurs="unbounded" minOccurs="0">
                <xsd:element name="analyticalInstrument">
                    <xsd:annotation>
                        <xsd:documentation>analytical instrument type used</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Potentiometer"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherAnalyticalInstrument" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of an analytical instrument that is not listed in ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element default="V" name="units">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="V"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <!--used for the description of a partial energy/enthaly type-->
    <xsd:complexType name="partialEnergyType">
        <xsd:annotation>
            <xsd:documentation>molar enthalpy of solution, partial molar enthalpy, relative
                partial molar enthalpy, partial molar Gibbs energy, relative partial molar Gibbs
                energy</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="J/mol">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="J/mol"/>
                        <xsd:enumeration value="J/kg"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="solute" minOccurs="1" maxOccurs="1" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>component linked to property </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="solutePhase" minOccurs="1" type="tml:phaseRefType">
                <xsd:annotation>
                    <xsd:documentation>phase linked to solute element added for dilution</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" name="solvent" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>solvent component(s) linked to property</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="AC calorimetry"/>
                            <xsd:enumeration value="closed cup calorimetry"/>
                            <xsd:enumeration value="drop calorimetry"/>
                            <xsd:enumeration value="drop ice or diphenyl ether calorimetry"/>
                            <xsd:enumeration value="DSC"/>
                            <xsd:enumeration value="DTA"/>
                            <xsd:enumeration value="flow calorimeter"/>
                            <xsd:enumeration value="large scale scanning calorimetry"/>
                            <xsd:enumeration value="laser pulse heating method"/>
                            <xsd:enumeration value="open cup calorimetry"/>
                            <xsd:enumeration value="resistive pulse heating"/>
                            <xsd:enumeration value="resistive pulse heating-millisecond"/>
                            <xsd:enumeration value="resistive pulse heating-microsecond"/>
                            <xsd:enumeration value="small adiabatic calorimetry"/>
                            <xsd:enumeration value="thin film calorimetry"/>
                            <xsd:enumeration value="vacuum adiabatic calorimetry"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in
                            ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

    <!--*****************************************-->
    <!--         Uncertainty definitions         -->
    <!--*****************************************-->
    <!--Uncertainty definitions-->
    <xsd:complexType name="uncertaintyType">
        <xsd:choice>
            <xsd:element name="confidenceInterval" type="tml:confidenceIntervalType"/>
            <xsd:element name="standardDeviation" type="tml:standardDeviationType"/>
            <xsd:element name="expandedUncertainty" type="tml:expandedUncertaintyType"/>
            <xsd:element name="other" type="tml:uncertaintyValueType"/>
            <xsd:element name="typeB" type="tml:uncertaintyValueType"/>
            <xsd:element name="combinedUncertainty" type="tml:uncertaintyValueType"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="uncertaintyValueType">
        <xsd:sequence>
            <xsd:element name="v" type="xsd:double"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="confidenceIntervalType">
        <xsd:complexContent>
            <xsd:extension base="tml:uncertaintyValueType">
                <xsd:attribute name="level">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:double">
                            <xsd:minInclusive value="0"/>
                            <xsd:maxInclusive value="1"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="standardDeviationType">
        <xsd:complexContent>
            <xsd:extension base="tml:uncertaintyValueType">
                <xsd:attribute name="type">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="mean"/>
                            <xsd:enumeration value="point"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="expandedUncertaintyType">
        <xsd:complexContent>
            <xsd:extension base="tml:uncertaintyValueType">
                <xsd:attribute name="combinedUncertainty">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:double"/>
                    </xsd:simpleType>
                </xsd:attribute>
                <xsd:attribute name="effectiveDegreesOfFreedom">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:double"/>
                    </xsd:simpleType>
                </xsd:attribute>
                <xsd:attribute name="confidenceLevel">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:double"/>
                    </xsd:simpleType>
                </xsd:attribute>
                <xsd:attribute name="coverageFactor">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:double"/>
                    </xsd:simpleType>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <!--**************************************-->
    <!--         equation definitions         -->
    <!--**************************************-->

    <!--equationComponentType fused to somehow link components to equations? -->
    <xsd:complexType name="equationComponentType">
        <xsd:sequence>
            <xsd:choice minOccurs="0">
                <xsd:element name="dataSetRef">
                    <xsd:annotation>
                        <xsd:documentation>a unique number for each instance of the pure or mixeture data block, used for external linking in equation representation. ThermoML 4.0 was nPureOrMixtureDataNumber</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="ref" type="xsd:positiveInteger"/>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="reactionDataSetRef">
                    <xsd:annotation>
                        <xsd:documentation>ThermoML 4.0 was nReactionDataNumber</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="ref" type="xsd:positiveInteger"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="symbol" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Used to map a symbol used in the equation definition (a ThermoMLEquation file) to a particular property defined in the ThermoML file. ThermoML 4.0 was sEqSymbol</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="index" type="xsd:integer">
                <xsd:annotation>
                    <xsd:documentation>for matrix representations.  used to map the property to a particular index in the equation definition. ThermoML 4.0 was nEqPropIndex
</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="otherUnit" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>allows the user to define any unit preferred for the property. Not a list due to plethora of possibilities. ThermoML 4.0 was sOtherPropUnit</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="minRange" type="xsd:float">
                <xsd:annotation>
                    <xsd:documentation>define the range within which the equation is valid for the particular property.  ThermoML 4.0 was nEqPropRangeMin</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="maxRange" type="xsd:float">
                <xsd:annotation>
                    <xsd:documentation>define the range within which the equation is valid for the particular property. ThermoML 4.0 was nEqPropRangeMax</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="criticalEvaluationType">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:choice minOccurs="0">
                <xsd:element name="multiPropertyList" type="xsd:string" maxOccurs="unbounded"/>
                <xsd:element name="equationOfStateName" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>description associated with equation of state</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="description" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>description associated with critical evaluation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="citation" minOccurs="0" maxOccurs="unbounded"
                type="tml:citationRefType">
                <xsd:annotation>
                    <xsd:documentation>link to citation associated with critical evaluation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="predictionType">
        <xsd:sequence>
            <xsd:element name="type">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="ab initio"/>
                        <xsd:enumeration value="molecular dynamics"/>
                        <xsd:enumeration value="semiempirical quantum methods"/>
                        <xsd:enumeration value="molecular mechanics"/>
                        <xsd:enumeration value="statistical mechanics"/>
                        <xsd:enumeration value="corresponding states"/>
                        <xsd:enumeration value="correlation"/>
                        <xsd:enumeration value="group contribution"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="name" type="xsd:string" minOccurs="0"/>
            <xsd:element name="description" type="xsd:string" minOccurs="0"/>
            <xsd:element name="citation" minOccurs="0" maxOccurs="unbounded"
                type="tml:citationRefType">
                <xsd:annotation>
                    <xsd:documentation>link to citation associated with prediction</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="ionicStrengthType">
        <!-- Restrict units to composition units, add relative to concepts for solutions a ratio measurements -->
        <xsd:annotation>
            <xsd:documentation>typically calculated as the product of a given ion's concentration, ci, and its charge, zi, summed over all ions in solution.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0" default="dimensionless"/>
            <xsd:element name="ion" minOccurs="1" maxOccurs="1" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>component measured</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="solutionIons"
                type="tml:chemicalRefType"/>
            <xsd:element name="units">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <!--unsure if this should be dimensionless or wt/at percent/fraction-->
                        <xsd:enumeration value="mol/kg"/>
                        <xsd:enumeration value="mol/m^3"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>method used for measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="electrolysis"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="phType">
        <!-- Restrict units to composition units, add relative to concepts for solutions a ratio measurements -->
        <xsd:annotation>
            <xsd:documentation>typically calculated as the log of (H+ concentration)</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0" default="dimensionless"/>
            <xsd:element name="units" default="unitless">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <!--unsure if this should be dimensionless or wt/at percent/fraction-->
                        <xsd:enumeration value="unitless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>method used for measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="pH meter"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="pCType">
        <!-- Restrict units to composition units, add relative to concepts for solutions a ratio measurements -->
        <xsd:annotation>
            <xsd:documentation>used in biostudies: typically calculated as the product of a given component's concentration, ci, summed over all components in solution.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0" default="dimensionless"/>
            <xsd:element name="component" type="tml:chemicalRefType"/>
            <xsd:element name="units" default="unitless">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <!--unsure if this should be dimensionless or wt/at percent/fraction-->
                        <xsd:enumeration value="unitless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>method used for measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="pH meter"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
