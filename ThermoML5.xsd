                            <xsd:element name="density">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="massDensity" type="tml:massDensityType">
                                            <xsd:annotation>
                                                <xsd:documentation>Mass of a sample or body divided by its volume. Also known as specific density
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="amountDensity"
                                            type="tml:amountConcentrationType">
                                            <xsd:annotation>
                                                <xsd:documentation>the amount of substance (mole) divided by the volume
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="enthalpy">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="apparentMolarEnthalpy"
                                            type="tml:apparentOrExcessEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>an enthalpy term developed by a process that is not well known or that carries uncertainties where are not known.
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessMolarEnthalpyMolarEnthalpyOfMixing"
                                            type="tml:apparentOrExcessEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference in measured/experimental and ideal internal energy of a system plus the product of pressure and volume per amount of substance (per mole solution) during mixing.
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarEnthalpy" type="tml:energyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the internal energy of a system plus the product of pressure and volume per amount of substance (in moles).  Its change in a system is equal to the heat brought to the system at constant pressure per amount of substance (in moles).
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarEnthalpyFunction"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>{Hm(T)-Hm(0)}/T</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarEnthalpyOfDilution"
                                            type="tml:partialEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the internal energy of a system plus the product of pressure and volume per amount of substance (per mole solute) added during the dilution of a solution.
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarEnthalpyOfMixingWithSolvent"
                                            type="tml:apparentOrExcessEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the internal energy of a system plus the product of pressure and volume per amount of substance (per mole of solute) added during mixing.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarEnthalpyOfSolution"
                                            type="tml:partialEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the internal energy of a system plus the product of pressure and volume per amount of substance (per mole of solute) added during the formation of a solution.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarEnthalpyOfTransitionOrFusion"
                                            type="tml:energyType">
                                            <xsd:annotation>
                                                <xsd:documentation>The quantity of enthalpy required to transform the phase of an amount of a component of a material</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarEnthalpyOfVaporizationOrSublimation"
                                            type="tml:energyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the enthalpy required to transform an amount of material from a liquid (vaporization) or solid (sublimation)
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="partialMolarEnthalpy"
                                            type="tml:partialEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>change in the enthalpy when an infinitesimal amount of substance (mole) is added to the system at constant temperature, pressure and amounts of all other constituents in the system divided by the added amount.
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="relativePartialMolarEnthalpy"
                                            type="tml:partialEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the quotient of the partial molar entropy in two different systems.
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="standardStateMolarEnthalpy"
                                            type="tml:energyType"/>
                                        <xsd:element name="vantHoffEnthalpyOfTransition"
                                            type="tml:energyType">
                                            <xsd:annotation>
                                                <xsd:documentation>"applicable to proteins, nucleic acid sequences and lipid assemblies.  The transition or calorimetric enthalpy, which is the heat capacity difference between the
sample and baseline integrated over the transitional temperature range per amount of protein in g
or mole undergoing the transition. "
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="entropy">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="apparentMolarEntropy"
                                            type="tml:entropyType">
                                            <xsd:annotation>
                                                <xsd:documentation>an entropy term developed by a process that is not well known or that carries uncertainties where are not known.
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessMolarEntropy"
                                            type="tml:entropyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference in measured/experimental and ideal enthalpy per amount of substance (mole)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="partialMolarEntropy"
                                            type="tml:entropyType">
                                            <xsd:annotation>
                                                <xsd:documentation>change in the entropy when an infinitesimal amount of substance (mole) is added to the system at constant temperature, pressure and amounts of all other constituents in the system divided by the added amount (mole)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="relativePartialMolarEntropy"
                                            type="tml:entropyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the quotient of the partial molar Gibbs energy in two different systems</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="standardStateMolarEntropy"
                                            type="tml:entropyType"/>
                                        <xsd:element name="molarEntropy" type="tml:entropyType">
                                            <xsd:annotation>
                                                <xsd:documentation>quantity the change in which is equal to the heat brought to the system in a reversible process at constant temperature divided by that temperature per amount of substance (mole)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarEntropyOfTransitionOrFusion"
                                            type="tml:entropyType">
                                            <xsd:annotation>
                                                <xsd:documentation>The quantity of entropy required to transform the phase of an amount of a component of a material (mole)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarEntropyOfVaporizationOrSublimation"
                                            type="tml:entropyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the entropy required to transform an amount of material (mole) from a liquid (vaporization) or solid (sublimation)
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="emittance">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <!-- Properties -->
                                        <!-- example unit:	Kelvin 			-->
                                        <xsd:element name="spectralHemisphericalEmittance"
                                            type="tml:spectralHemisphericalEmittanceType">
                                            <xsd:annotation>
                                                <xsd:documentation>Ratio of radiant exitance of a thermal radiator to that of a full radiator (black body) at the same temperature for a known wavelength, integrated over all angles to the surface of the material
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	dimensionless	-->
                                        <xsd:element name="spectralNormalEmittance"
                                            type="tml:spectralNormalEmittanceType">
                                            <xsd:annotation>
                                                <xsd:documentation>Ratio of radiant exitance of a thermal radiator to that of a full radiator (black body) at the same temperature for a known wavelength, at an angle orthogonal to the surface of the  material</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	dimensionless	-->
                                        <xsd:element name="totalHemisphericalEmittance"
                                            type="tml:totalHemisphericalEmittanceType">
                                            <xsd:annotation>
                                                <xsd:documentation>Ratio of radiant exitance of a thermal radiator to that of a full radiator (black body) at the same temperature  integrated over  the whole wavelength range, and over all an angles relative to the surface</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="totalNormalEmittance"
                                            type="tml:totalNormalEmittanceType">
                                            <xsd:annotation>
                                                <xsd:documentation>Ratio of radiant exitance of a thermal radiator to that of a full radiator (black body) at the same temperature integrated over  the whole wavelength range, at an angle orthogonal to the
 surface</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="expansionCoefficient">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="thermalPressureCoefficient"
                                            type="tml:thermalPressureCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a quantity that characterizes the change in pressure at constant volume induced by changes in temperature</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="isobaricCoefficientOfExpansion"
                                            type="tml:thermalExpansionCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>the measure of how much a volume of matter changes when placed under temperatures at constant pressure, normalized to a reference volume. Commonly referred to as the Linear Coefficient of Expansion. Alpha=1/V (dV/dT)_P
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessCoefficientOfExpansion"
                                            type="tml:thermalExpansionCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference between real/measured and ideal coefficients of expansion
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="linearExpansionCoefficient"
                                            type="tml:linearExpansionCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>Similar to isobaric Coefficient of expansion, though was measured in one orientation.  alpha= (L-L0) / (L0* delta T)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="relativeLinearExpansion"
                                            type="tml:relativeLinearExpansionType">
                                            <xsd:annotation>
                                                <xsd:documentation>a measure of the change in length of a specimen compared to a reference geometry at specific conditions (T, P, etc.).  RLE= (L- L0)/L0
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="relativeVolumetricExpansion"
                                            type="tml:relativeVolumetricExpansionType">
                                            <xsd:annotation>
                                                <xsd:documentation>a measure of the change in volume of a specimen compared to a reference geometry at specific conditions (T, P, etc.).  VLE= (V-V0)/V0
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="gasSolubilityCoefficient">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="BunsenCoefficient"
                                            type="tml:bunsenCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>the volume of gas reduced to STP which is absorbed by a unit volume of solvent at the temperature and pressure of measurement) under a partial pressure of 101325 Pa</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="OstwaldCoefficient"
                                            type="tml:ostwaldCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>the ratio of the volume of a gas absorbed to the volume of the absorbing liquid, all measured at the same temperature
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="gibbsEnergy">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="apparentMolarGibbsEnergy"
                                            type="tml:apparentOrExcessEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>an energy term developed by a process that is not well known or that carries uncertainties where are not known</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessMolarGibbsEnergy"
                                            type="tml:energyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference in measured/experimental and ideal Gibbs free energy per amount of substance (mole)
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="partialMolarGibbsEnergy"
                                            type="tml:partialEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>change in the Gibbs energy when an infinitesimal amount of substance (mole) is added to the system at constant temperature, pressure and amounts of all other constituents in the system divided by the added amount of substance (mole)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="relativePartialMolarGibbsEnergy"
                                            type="tml:partialEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the quotient of the partial molar heat capacity in two different systems</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="standardStateMolarGibbsEnergy"
                                            type="tml:energyType">
                                            <xsd:annotation>
                                                <xsd:documentation/>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarGibbsEnergyFunction"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>{Gm(T)-Hm(0)}/T</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="standardMolarGibbsEnergyFunction"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>{Gm(T)-Hm(298.15)}/T</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarGibbsEnergy" type="tml:energyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the quantity expressed by the enthalpy minus the product of the thermodynamic temperature and entropy per amount of substance (mole). (del G) = (del H) - T (del S)
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element
                                            name="molarGibbsEnergyOfVaporizationOrSublimation"
                                            type="tml:energyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the Gibbs energy required to transform an amount of material from a liquid (vaporization) or solid (sublimation)
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="helmholtzEnergy">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="molarHelmholtzEnergy"
                                            type="tml:energyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the quantity expressed by the internal energy minus the product of thermodynamic temperature and entropy per amount of substance (mole)
(mole)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="henrysLawConstant">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="henrysLawConstantMoleFractionScale"
                                            type="tml:pressureType">
                                            <xsd:annotation>
                                                <xsd:documentation>a constant (proportionality factor) relating the amount of a dissolved gas in a liquid to its partial pressure in the gas phase.  OR the ratio of the aqueous phase concentration (mole fraction) to its equilibrium partial pressure in the gas phase 
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="henrysLawConstantMolalityScale"
                                            type="tml:henrysLawConstantMolalityType">
                                            <xsd:annotation>
                                                <xsd:documentation>a constant (proportionality factor) relating the amount of a dissolved gas in a liquid to its partial pressure in the gas phase.  OR the ratio of the aqueous phase concentration (molality) to its equilibrium partial pressure in the gas phase 
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element
                                            name="henrysLawConstantAmountConcentrationScale"
                                            type="tml:henrysLawConstantMolarityType">
                                            <xsd:annotation>
                                                <xsd:documentation>a constant (proportionality factor) relating the amount of a dissolved gas in a liquid to its partial pressure in the gas phase.  OR the ratio of the aqueous phase concentration (amount concentration) to its equilibrium partial pressure in the gas phase 
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="heatCapacity">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="apparentMolarHeatCapacity"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>an heat capacity term developed by a process that is not well known or that carries uncertainties where are not known</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessMolarHeatCapacity"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference in measured/experimental and ideal  heat capacity per amount of substance (mole)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="heatCapacityAtConstantPressurePerVolume"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the heat brought to a system to increase its temperature divided by that temperature increase divided by volume at constant pressure
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="heatCapacityAtConstantVolumePerVolume"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the heat brought to a system to increase its temperature divided by that temperature increase divided by volume at constant volume
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="heatCapacityChangeAtTransition"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>applicable to proteins, nucleic acid sequences and lipid assemblies. The difference between the extrapolated pre- and post transitional baselines at the transition temperature, i.e., the difference between the heat capacities of the protein in its native state and unfolded state</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element
                                            name="heatCapacityAtSaturationPressurePerVolume"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the heat capacity at saturation per unit volume, where saturation is defined as: saturation solution: A solution which has the same concentration of a solute as one that is in equilibrium with undissolved solute at specified values of the temperature and pressure</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="heatCapacityOverEmittance"
                                            type="tml:heatCapacityOverEmittanceType"/>
                                        <xsd:element name="heatCapacityRatioCpOverCv"
                                            type="tml:heatCapacityRatioType">
                                            <xsd:annotation>
                                                <xsd:documentation>the ratio of heat capacity at constant pressure over the heat capacity at constant volume
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarHeatCapacityAtConstantPressure"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the heat brought to a system to increase its temperature divided by that temperature increase per amount of substance in the system (in moles) at constant pressure
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarHeatCapacityAtConstantVolume"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the heat brought to a system to increase its temperature divided by that temperature increase per amount in the system at constant volume
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarHeatCapacityAtSaturationPressure"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the molar heat capacity at saturation pressure, where saturation is defined as: A solution which has the same concentration of a solute as one that is in equilibrium with undissolved solute at specified values of the temperature and pressure</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="partialMolarHeatCapacity"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>change in the heat capacity when an infinitesimal amount of substance (mole) is added to the system at constant temperature, pressure and amounts of all other constituents in the system divided by the added amount</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="relativePartialMolarHeatCapacity"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the quotient of the partial molar enthalpy in two different systems</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="specificHeatCapacityAtConstantPressure"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the heat brought to a system to increase its temperature divided by that temperature increase divided by mass at constant pressure
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="specificHeatCapacityAtSaturationPressure"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the specific heat capacity at saturation pressure, where saturation is defined as: saturation solution: A solution which has the same concentration of a solute as one that is in equilibrium with undissolved solute at specified values of the temperature and pressure</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="specificHeatCapacityAtConstantVolume"
                                            type="tml:heatCapacityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the heat brought to a system to increase its temperature divided by that temperature increase divided by mass at constant volume
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="standardStateMolarHeatCapacity"
                                            type="tml:heatCapacityType"/>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="internalEnergy">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="molarInternalEnergy"
                                            type="tml:energyType">
                                            <xsd:annotation>
                                                <xsd:documentation>the quantity expressed by the change in which is equal ot the sum of heat, q, brought to the system and work, w, performed on it, delta U=q+w per amount of substance (mole)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="latticeParameter">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <!-- Properties -->
                                        <xsd:element name="latticeParameterA"
                                            type="tml:latticeParameterType">
                                            <xsd:annotation>
                                                <xsd:documentation>a translation vector which maps the crystal lattice onto itself</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	kX 				-->
                                        <xsd:element name="latticeParameterB"
                                            type="tml:latticeParameterType">
                                            <xsd:annotation>
                                                <xsd:documentation>a translation vector which maps the crystal lattice onto itself</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	kX 				-->
                                        <xsd:element name="latticeParameterC"
                                            type="tml:latticeParameterType">
                                            <xsd:annotation>
                                                <xsd:documentation>a translation vector which maps the crystal lattice onto itself</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	kX 				-->
                                        <xsd:element name="LatticeAngleAlpha"
                                            type="tml:latticeAngleType">
                                            <xsd:annotation>
                                                <xsd:documentation>interaxial lattice angle b^c
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	rad 			-->
                                        <xsd:element name="LatticeAngleBeta"
                                            type="tml:latticeAngleType">
                                            <xsd:annotation>
                                                <xsd:documentation>interaxial lattice angle c^a
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	rad 			-->
                                        <xsd:element name="LatticeAngleGamma"
                                            type="tml:latticeAngleType">
                                            <xsd:annotation>
                                                <xsd:documentation>interaxial lattice angel a^b
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	rad 			-->
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="molarLatticeEnergyat0K">
                                <xsd:annotation>
                                    <xsd:documentation/>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="molarLatticeEnergyAt0K"
                                            type="tml:energyType">
                                            <xsd:annotation>
                                                <xsd:documentation>The (typically predicted or extrapolated) minimum energy of a lattice for one mole of a component</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="osmoticProperties">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="osmoticPressure" type="tml:pressureType">
                                            <xsd:annotation>
                                                <xsd:documentation>excess pressure required to maintain osmotic equilibrium between a solution and the pure solvent separated by a membrane permeable to only the solvent</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="osmoticCoefficient"
                                            type="tml:osmoticCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>Quantity characterizing the deviation of the solvent from ideal behaviour referenced to Raoult's law</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="permeability">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="magneticPermeability"
                                            type="tml:magneticPermeabilityType">
                                            <xsd:annotation>
                                                <xsd:documentation>a constant of proportionality that exists between magnetic induction and magnetic field intensity</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="relativeMagneticPermeability"
                                            type="tml:relativeMagneticPermeabilityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the ratio of the magnetic permeability of a specific material normalized by the magnetic permeability of free space. It is related to the magnetic susceptibility of a material</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="permittivty">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <!-- Properties -->
                                        <!-- example unit:	dimensionless 		-->
                                        <!-- example unit:	dimensionless 		-->
                                        <!-- example unit:	dimensionless 		-->
                                        <!-- example unit:	m^3/mol 			-->
                                        <xsd:element name="relativePermittivityAtZeroFrequency"
                                            type="tml:relativePermittivityType">
                                            <xsd:annotation>
                                                <xsd:documentation>Ratio of the electric field strength in vacuum to that in a given medium at zero frequency. It was formerly called the dielectric constant</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	dimensionless 		-->
                                        <xsd:element
                                            name="excessRelativePermittivityAtZeroFrequency"
                                            type="tml:permittivityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference of the measured/experimental and ideal relative permittivity, where the relative permittivity is defined as: Ratio of the electric field strength in vacuum to that in a given medium at zero frequency. It was formerly called the dielectric constant</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	dimensionless 		-->
                                        <xsd:element name="relativePermittivityAtVariousFrequencies"
                                            type="tml:relativePermittivityType">
                                            <xsd:annotation>
                                                <xsd:documentation>Ratio of the electric field strength in vacuum to that in a given medium at a specified frequency. It was formerly called the dielectric constant</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	dimensionless 		-->
                                        <xsd:element
                                            name="complexRelativePermittivityRealPartAtVariousFrequencies"
                                            type="tml:complexRelativePermittivityType">
                                            <xsd:annotation>
                                                <xsd:documentation>Ratio of the real components of an electric field strength in vacuum to that in a given medium at a specified frequency</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	dimensionless 		-->
                                        <xsd:element
                                            name="complexRelativePermittivityImaginaryPartAtVariousFrequencies"
                                            type="tml:complexRelativePermittivityType">
                                            <xsd:annotation>
                                                <xsd:documentation>Ratio of the imaginary components of an electric field strength in vacuum to that in a given medium at a specified frequency</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <!-- example unit:	dimensionless 		-->
                                        <!-- example unit:	J/m^2 				-->
                                        <!-- example unit:	J/m^2 				-->
                                        <!-- example unit:	J/m^2 				-->
                                        <!-- example unit:	m/s 				-->
                                        <!-- example unit:	m/s 				-->
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="surfaceTension">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="surfaceTensionLiquidGas"
                                            type="tml:surfaceTensionType">
                                            <xsd:annotation>
                                                <xsd:documentation>quantity representing the work required to increase a surface area of a liquid divided by that area in a gaseous environment
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessSurfaceTensionLiquidGas"
                                            type="tml:surfaceTensionType">
                                            <xsd:annotation>
                                                <xsd:documentation>difference of the measured/experimental and ideal surface tension between a liquid and a gas</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="interfacialTension"
                                            type="tml:surfaceTensionType">
                                            <xsd:annotation>
                                                <xsd:documentation>quantity representing the work required to increase a surface area between two condensed phases (liquid+liquid, liquid+solid, or solid+solid) divided by that area</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="virialCoefficient">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="secondVirialCoefficient"
                                            type="tml:secondVirialCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a coefficient in the virial equation of state for a real gas</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="secondAcousticVirialCoefficient"
                                            type="tml:secondVirialCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a coefficient in the virial equation of state for a real gas</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="thirdVirialCoefficient"
                                            type="tml:thirdVirialCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a coefficient in the virial equation of state for a real gas</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="thirdAcousticVirialCoefficient"
                                            type="tml:thirdVirialCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a coefficient in the virial equation of state for a real gas</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="thirdInteractionVirialCoefficientC112"
                                            type="tml:thirdVirialCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a coefficient in the virial equation of state for a real gas</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="thirdInteractionVirialCoefficientC122"
                                            type="tml:thirdVirialCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a coefficient in the virial equation of state for a real gas</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessVirialCoefficient"
                                            type="tml:secondVirialCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a coefficient in the virial equation of state for a real gas</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="interactionVirialCoefficient"
                                            type="tml:secondVirialCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a coefficient in the virial equation of state for a real gas</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="volume">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="specificVolume"
                                            type="tml:specificVolumeType">
                                            <xsd:annotation>
                                                <xsd:documentation>volume of a substance divided by its mass.  It is the reciprocal of mass density</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarVolume" type="tml:molarVolumeType">
                                            <xsd:annotation>
                                                <xsd:documentation>the volume divided by amount of substance (mole)
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="apparentMolarVolume"
                                            type="tml:relativeMolarVolumeType">
                                            <xsd:annotation>
                                                <xsd:documentation>the change in volume when a component is added to a solution per mole of component added at a specific composition, at infinite dilution it is equal to the partial molar volume. xi= 1/V (dV/dP)_S</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessMolarVolume"
                                            type="tml:molarVolumeType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference between real/measured and ideal molar volume (a measure of non-ideal behavior)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="partialMolarVolume"
                                            type="tml:partialMolarVolumeType">
                                            <xsd:annotation>
                                                <xsd:documentation>change in volume when an infinitesimal amount of substance (mole) is added to the system at constant temperature, pressure and amounts of all other constituents in the system divided by the added amount
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="relativePartialMolarVolume"
                                            type="tml:relativeMolarVolumeType">
                                            <xsd:annotation>
                                                <xsd:documentation>quotient of change in volume when an infinitesimal amount of substance (mole) is added to the system at constant temperature, pressure, and amounts of all other constituents in the system divided by the added amounts between two different systems
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:choice>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="transport">
                    <xsd:annotation>
                        <xsd:documentation>properties associated with the transport of mass, energy, current</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:choice>
                            <xsd:element name="diffusionCoefficient">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="binaryDiffusionCoefficient"
                                            type="tml:diffusionCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a proportionally constant relating the flux of one component in a solution consisting of similar or other components.
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="selfDiffusionCoefficient"
                                            type="tml:diffusionCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a proportionally constant relating the flux of one component in a solution consisting of similar components.
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="tracerDiffusionCoefficient"
                                            type="tml:diffusionCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>a proportionally constant relating the flux of a foreign substance mixed or attached to a substance to enable the distribution or location of the latter be determined subsequently.
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="electricalTransport">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="electricalResistivityAtReferenceGeometry"
                                            type="tml:electricalResistivityType">
                                            <xsd:annotation>
                                                <xsd:documentation>Electric field strength divided by the current density. This quantity is a tensor in an anisotropic medium. Uncorrected for geometric effects (thermal expansion, etc.), relative to a specific T,P,etc.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="electricalResisitivity"
                                            type="tml:electricalResistivityType">
                                            <xsd:annotation>
                                                <xsd:documentation>Electric field strength divided by the current density. This quantity is a tensor in an anisotropic medium. Corrected for geometric effects (thermal expansion, etc.)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="electricalConductivity"
                                            type="tml:electricalConductivityType">
                                            <xsd:annotation>
                                                <xsd:documentation>a measure of the ability of a material to conduct current, the inverse of electrical resistivity</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarConductivity"
                                            type="tml:molarConductivityType">
                                            <xsd:annotation>
                                                <xsd:documentation>conductivity of an electrolyte solution divided by the amount concentration of the electrolyte.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="transportNumber"
                                            type="tml:transportNumberType">
                                            <xsd:annotation>
                                                <xsd:documentation>For ions, formerly transference number. Transport number is defined as the fraction of the total current that is carried by a specific ion during electrolysis. (also known as transference number, Hittorf number or current fraction).  Of ions B, the current density due to ions B divided by the sum of current densities of all the ions in the electrolyte. ISO recommends the name current fraction of ions B. 
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="refractiveIndex">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="refractiveIndexNaDLine"
                                            type="tml:refractiveIndexType">
                                            <xsd:annotation>
                                                <xsd:documentation>the ratio of the speed of light in vacuum to that in of the measured medium, done at the yellow doublet D-line of sodium with a wavelength of 589 nm
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessRefractiveIndexNaDLine"
                                            type="tml:refractiveIndexType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference of the measured/experimental and ideal ratio of the speed of light in vacuum to that in of the measured medium, done at the yellow doublet D-line of sodium with a wavelength of 589 nm
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="refractiveIndexOtherWavelength"
                                            type="tml:otherRefractiveIndexType">
                                            <xsd:annotation>
                                                <xsd:documentation>the ratio of the speed of light in vacuum to that in of the measured medium, done at a specified wavelength (not 589nm)
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessRefractiveIndexOtherWavelength"
                                            type="tml:otherRefractiveIndexType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference of the measured/experimental and ideal ratio of the speed of light in vacuum to that in of the measured medium, done at a specified wavelength (not 589nm)
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="speedOfSound">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="fluidSpeedOfSound"
                                            type="tml:fluidSpeedOfSoundType">
                                            <xsd:annotation>
                                                <xsd:documentation>the measured scalar quantity equal to the absolute value of the velocity of a sound wave as it propagates through the liquid medium
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessFluidSpeedOfSound"
                                            type="tml:fluidSpeedOfSoundType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference between real/measured and ideal speed of sound (a measure of non-ideal behavior)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="thermalConductivity"
                                type="tml:thermalConductivityType">
                                <xsd:annotation>
                                    <xsd:documentation>Tensor quantity relating the heat flux, J q, to the temperature gradient, J q =    T. 
</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="thermalDiffusivity" type="tml:thermalDiffusivityType">
                                <xsd:annotation>
                                    <xsd:documentation>a measure of the rate of heat transfer of heat in a material from a warmer to a cooler location, alpha = k / p Cp</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="viscosity">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="dynamicViscosity"
                                            type="tml:viscosityType">
                                            <xsd:annotation>
                                                <xsd:documentation>for laminar flow of a fluid the ratio of the shear stress to the velocity gradient perpendicular to the plane of shear</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="viscosityDeviation"
                                            type="tml:viscosityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference between the real/measured and ideal dynamic viscosity. It is a measure of non-ideal behavior. also known as excess viscosity</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="kinematicViscosity"
                                            type="tml:kinematicViscosityType">
                                            <xsd:annotation>
                                                <xsd:documentation>dynamic viscosity divided by the density of the fluid</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="fluidity" type="tml:fluidityType">
                                            <xsd:annotation>
                                                <xsd:documentation>reciprocal of dynamic viscosity</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:choice>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="phaseTransitionConditions">
                    <xsd:annotation>
                        <xsd:documentation>properties associated with the conditions (temperature,pressure,composition) associated with a phase transition (monotectic, peritectic, triple point, etc.)</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:choice>
                                <xsd:element name="composition">
                                    <xsd:annotation>
                                        <xsd:documentation>(DEPRICATED)  </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                        <xsd:choice>
                                            <xsd:element name="massFraction">
                                                <xsd:complexType>
                                                  <xsd:choice>
                                                  <xsd:element
                                                  name="azeotropicCompositionMassFraction"
                                                  type="tml:massFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mass fraction composition at which a mixture of two liquids will evaporate/boil at, where evaporation is defined by IUPAC as the physical process by which a liquid substance is converted to a gas or vapor. 
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element
                                                  name="lowerConsoluteCompositionMassFraction"
                                                  type="tml:massFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mass fraction composition of a phase at which a critical solution point occurs.  Where the critical solution point is defined by IUPAC as the point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur.
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element
                                                  name="upperConsoluteCompositionMassFraction"
                                                  type="tml:massFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mass fraction composition of a phase at which a critical solution point occurs.  Where the critical solution point is defined by IUPAC as the point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur.
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element
                                                  name="eutecticCompositionMassFraction"
                                                  type="tml:massFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mass fraction composition of a phase eat which a eutectic reaction occurs. Where a eutectic reaction is defined by IUPAC as: an isothermal, reversible reaction between two (or more) solid phases during the heating of a system, as a result of which a single liquid phase is produced.
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element
                                                  name="monotecticCompositionMassFraction"
                                                  type="tml:massFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mass fraction composition of a phase at which a monotectic reaction occurs. Where a monotectic reaction is defined by IUPAC as: the reversible transition, on cooling, of a liquid to a mixture of a second liquid and a solid.
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="massFractionInLLGCriticalState"
                                                  type="tml:massFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mass fraction of a component at the critical point in a three phase mixture of two liquids and a gas phase.  Where the critical point is defined by the temperature and pressure at which the liquid and vapor intensive properties (density, heat capacity, etc.) become equal. It is the highest temperature (critical temperature) and pressure (critical pressure) at which both a gaseous and a liquid phase of a given compound can coexist
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:choice>
                                                </xsd:complexType>
                                            </xsd:element>
                                            <xsd:element name="moleFraction">
                                                <xsd:complexType>
                                                  <xsd:choice>
                                                  <xsd:element
                                                  name="azeotropicCompositionMoleFraction"
                                                  type="tml:moleFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mole fraction composition at which a mixture of two liquids will evaporate/boil at, where evaporation is defined by IUPAC as the physical process by which a liquid substance is converted to a gas or vapor. 
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element
                                                  name="lowerConsoluteCompositionMoleFraction"
                                                  type="tml:moleFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mole fraction composition of a phase at which a critical solution point occurs.  Where the critical solution point is defined by IUPAC as the point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur.
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element
                                                  name="upperConsoluteCompositionMoleFraction"
                                                  type="tml:moleFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mole fraction composition of a phase at which a critical solution point occurs.  Where the critical solution point is defined by IUPAC as the point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur.
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element
                                                  name="eutecticCompositionMoleFraction"
                                                  type="tml:moleFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mole fraction composition of a phase at which a eutectic reaction occurs. Where a eutectic reaction is defined by IUPAC as: an isothermal, reversible reaction between two (or more) solid phases during the heating of a system, as a result of which a single liquid phase is produced.
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element
                                                  name="monotecticCompositionMoleFraction"
                                                  type="tml:moleFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mole fraction composition of a phase at which a monotectic reaction occurs. Where a monotectic reaction is defined by IUPAC as: the reversible transition, on cooling, of a liquid to a mixture of a second liquid and a solid.
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element name="moleFractionInLLGCriticalState"
                                                  type="tml:moleFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The mole fraction of a component at the critical point in a three phase mixture of two liquids and a gas phase.  Where the critical point is defined by the temperature and pressure at which the liquid and vapor intensive properties (density, heat capacity, etc.) become equal. It is the highest temperature (critical temperature) and pressure (critical pressure) at which both a gaseous and a liquid phase of a given compound can coexist
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:choice>
                                                </xsd:complexType>
                                            </xsd:element>
                                            <xsd:element name="volumeFraction">
                                                <xsd:complexType>
                                                  <xsd:choice>
                                                  <xsd:element
                                                  name="lowerConsoluteCompositionVolumeFraction"
                                                  type="tml:volumeFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The volume fraction composition of a phase at which a critical solution point occurs.  Where the critical solution point is defined by IUPAC as the point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur.
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element
                                                  name="upperConsoluteCompositionVolumeFraction"
                                                  type="tml:volumeFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The Volume fraction composition of a phase at which a critical solution point occurs.  Where the critical solution point is defined by IUPAC as the point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur.
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  <xsd:element
                                                  name="eutecticCompositionVolumeFraction"
                                                  type="tml:volumeFractionType">
                                                  <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The volume fraction composition of a phase at which a eutectic reaction occurs. Where a eutectic reaction is defined by IUPAC as: an isothermal, reversible reaction between two (or more) solid phases during the heating of a system, as a result of which a single liquid phase is produced.
</xsd:documentation>
                                                  </xsd:annotation>
                                                  </xsd:element>
                                                  </xsd:choice>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </xsd:choice>
                                    </xsd:complexType>
                                </xsd:element>
                                <xsd:element name="criticalCondition">
                                    <xsd:annotation>
                                        <xsd:documentation>(DEPRICATED)  </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                        <xsd:choice>
                                            <xsd:element name="criticalCompressibilityFactor"
                                                type="tml:compressibilityFactorType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  Compressibility factor at the Critical Point. Where the critical point is defined by IUPAC as: The temperature and pressure at which the liquid and  vapor intensive properties (density, heat capacity, etc.) become equal. It is the highest temperature (critical temperature) and pressure (critical pressure) at which both a gaseous and a liquid phase of a given compound can coexist.

</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="criticalMassDensity"
                                                type="tml:massDensityType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  Mass density at the Critical Point. Where the critical point is defined by IUPAC as: The temperature and pressure at which the liquid and vapor intensive properties (density, heat capacity, etc.) become equal. It is the highest temperature (critical temperature) and pressure (critical pressure) at which both a gaseous and a liquid phase of a given compound can coexist.</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="criticalMolarVolume"
                                                type="tml:molarVolumeType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  Molar volume at the Critical Point. Where the critical point is defined by IUPAC as: The temperature and pressure at which the liquid and  vapor intensive properties (density, heat capacity, etc.) become equal. It is the highest temperature (critical temperature) and pressure (critical pressure) at which both a gaseous and a liquid phase of a given  compound can coexist.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="criticalPressure"
                                                type="tml:pressureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The minimum pressure which would suffice to liquefy a substance at its critical temperature. Above the critical pressure, increasing the temperature will not cause a fluid to vaporize to give a two-phase system.</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="criticalSpecificVolume"
                                                type="tml:massSpecificVolumeType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  Specific volume at the Critical Point. Where the critical point is defined by IUPAC as: The temperature and pressure at which the liquid and    vapor intensive properties (density, heat capacity,  etc.) become equal. It is the highest temperature (critical temperature) and pressure (critical pressure)   at which both a gaseous and a liquid phase of a given compound can coexist.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="criticalTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  That temperature, characteristic of each gas, above which it is not possible to liquefy a given gas.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                        </xsd:choice>
                                    </xsd:complexType>
                                </xsd:element>
                                <xsd:element name="pressure">
                                    <xsd:annotation>
                                        <xsd:documentation>(DEPRICATED)  </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                        <xsd:choice>
                                            <xsd:element name="azeotropePressure"
                                                type="tml:pressureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The pressure at which a mixture of two liquids will evaporate/boil at a constant composition, where evaporation is defined by IUPAC as the physical process by which a liquid substance is converted to a gas or vapor. 
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="lowerConsolutePressure"
                                                type="tml:pressureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  Pressure at the critical solution point.  Where the critical solution point is defined by IUPAC as the point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="partialPressure"
                                                type="tml:partialPressureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  For a mixture of gases the contribution by each constituent is called the partial pressure pi= xi p, where xi is the amount fraction of the ith constituent and p is the total  pressure.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="phaseBoundaryPressure"
                                                type="tml:pressureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  equilibrium pressure of a closed system with two  phases present</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="quadrupleOrQuintuplePointPressure"
                                                type="tml:pressureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  Pressure at a quadruple or quintuple point similar to a triple point. Where the triple (3), quadruple (4) , or quintuple (5) point can  be defined using the Gibbs phase Rule In a system with  P phases, C components, and F degrees of a freedom related by P+F=C+2 (assuming pressure is constant).
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="triplePointPressure"
                                                type="tml:pressureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  Pressure at a triple point. Where the triple point is defined by IUPAC as: The point in a one-component system at which the temperature and pressure of three phases are in equilibrium. If there are  p possible phases, there are  p ! ( p  3 ) ! 3 ! triple points.</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="upperConsolutePressure"
                                                type="tml:pressureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  Pressure at the critical solution point.  Where the critical solution point is defined by IUPAC as the point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="vaporPressure"
                                                type="tml:pressureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>(DEPRICATED)  The equilibrium pressure of a vapor component above a liquid or solid. The pressure at which a component in a system will evaporate or sublimate.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                        </xsd:choice>
                                    </xsd:complexType>
                                </xsd:element>
                                <xsd:element name="temperature">
                                    <xsd:annotation>
                                        <xsd:documentation>(DEPRICATED)  </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                        <xsd:choice>
                                            <xsd:element name="azeotropeTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>the temperature at which a mixture of two liquids will evaporate/boil at a constant composition, where evaporation is defined by IUPAC as the physical process by which a liquid substance is converted to a gas or vapor. 
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="boilingTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>Temperature at which evaporation occurs, where evaporation is defined by IUPAC as the physical process by which a liquid substance is converted to a gas or vapor. 
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="eutecticTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>The temperature at which a eutectic reaction occurs. Where a eutectic reaction is defined by IUPAC as: an isothermal, reversible reaction between two (or more) solid phases during the heating of a system, as a result of which a single liquid phase is produced.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="liquidLiquidEquilibriumTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>temperature at which coexistance of two liquid phases occurs for a prescribed composition for one of the liquids
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="lowerConsoluteTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>Temperature at the critical solution point.  Where the critical solution point is defined by IUPAC as the point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="monotecticTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>The temperature at which a monotectic reaction occurs. Where a monotectic reaction is defined by IUPAC as: the reversible transition, on cooling, of a liquid to a mixture of a second liquid and a solid.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="normalBoilingTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>Temperature at which evaporation occurs at 101325 Pa pressure, where evaporation is defined by IUPAC as the physical process by which a liquid substance is converted to a gas or vapor. </xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="normalMeltingTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>Temperature at a melting point at 101325 Pa pressure, where melting is defined by IUPAC as the conversion of a solid to a liquid by the application of heat and/or pressure
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="peakTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>applicable to proteins, nucleic acid sequences and lipid assemblies. the temperature of the maximum transition peak</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="peritecticTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>The temperature at which a peritectic reaction occurs. Where a peritectic reaction is defined by IUPAC as: an isothermal, reversible reaction between two phases, a liquid and a solid, that results, on cooling of a binary, ternary, ... ,  n system in one, two, ... (n  1) new solid phases. Synonymous with incongruent reaction.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="quadrupleOrQuintuplePointTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>Temperature at a quadruple or quintuple point similar to a triple point. Where the triple (3), quadruple (4) , or quintuple (5) point.  Which can be defined using the Gibbs Phase Rule In a system with P phases, C components, and F degrees of a freedom related by P+F=C+2 (assuming pressure is constant).
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="solidLiquidEquilibriumTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>temperature at which coexistance of solid and liquid occurs for a prescribed composition of solid.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="temperatureOfHalfConversion"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>applicable to proteins, nucleic acid sequences and lipid assemblies. The temperature where 50% of the protein population is unfolded
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="triplePointTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>Temperature at a triple point. Where the triple point is defined by IUPAC as: The point in a one-component system at which the temperature and pressure of three phases are in equilibrium. If there are  p possible phases, there are  p ! ( p  3 ) ! 3 ! triple points.</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="upperConsoluteTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>Temperature at the critical solution point.  Where the critical solution point is defined by IUPAC as the point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur.
</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="zeroGibbsEnergyTemperature"
                                                type="tml:temperatureType">
                                                <xsd:annotation>
                                                  <xsd:documentation>applicable to proteins, nucleic acid sequences and lipid assemblies. The temperature where the standard molar Gibbs energy change of the transition is equal to zero</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                        </xsd:choice>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:choice>
                            <xsd:choice>
                                <xsd:element name="method">
                                    <xsd:annotation>
                                        <xsd:documentation>Method for phase equilibria determination, note there is a long list of "all methods" used from those Types found in the phase eq. properties commented out.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                        <xsd:restriction base="xsd:string">
                                            <xsd:enumeration
                                                value="azeotropic T or P determination when X=Y"/>
                                            <xsd:enumeration
                                                value="azeotropic T or P determination by T or P extreme"/>
                                            <xsd:enumeration value="closed-cell static method"/>
                                            <xsd:enumeration value="chromatography"/>
                                            <xsd:enumeration
                                                value="calculated by Gibbs-Duhem equation"/>
                                            <xsd:enumeration value="calorimetry, static"/>
                                            <xsd:enumeration value="calorimetry, adiabatic"/>
                                            <xsd:enumeration
                                                value="calorimetry, adiabatic small sample "/>
                                            <xsd:enumeration value="calorimetry, vacuum adiabatic"/>
                                            <xsd:enumeration value="calorimetry, flow"/>
                                            <xsd:enumeration value="calorimetry, drop"/>
                                            <xsd:enumeration
                                                value="calorimetry, drop -ice or diphenyl ether"/>
                                            <xsd:enumeration
                                                value="calorimetry, DSC- differential scanning calorimetry"/>
                                            <xsd:enumeration value="cooling or heating curves"/>
                                            <xsd:enumeration
                                                value="depression of a freezing temperature of a dilute solution"/>
                                            <xsd:enumeration
                                                value="derived, extrapolated vapor pressure"/>
                                            <xsd:enumeration
                                                value="derived, from electrochemical cell potential"/>
                                            <xsd:enumeration
                                                value="derived from Knudsen effusion weight loss"/>
                                            <xsd:enumeration
                                                value="derived, from phase diagram analysis"/>
                                            <xsd:enumeration value="derived, from PVT data"/>
                                            <xsd:enumeration
                                                value="derived, from rectilinear diameter"/>
                                            <xsd:enumeration value="derived, from second law"/>
                                            <xsd:enumeration
                                                value="determined from refractive index and/or density"/>
                                            <xsd:enumeration
                                                value="determined from change in volume/density"/>
                                            <xsd:enumeration
                                                value="determined from change in resistivity"/>
                                            <xsd:enumeration
                                                value="determined from refractive permittivity"/>
                                            <xsd:enumeration
                                                value="determined from incramental heat treatment, quenching, and metallography"/>
                                            <xsd:enumeration value="distillation"/>
                                            <xsd:enumeration value="DSC/DTA"/>
                                            <xsd:enumeration value="dynamic method"/>
                                            <xsd:enumeration value="ebuilliometry, differential "/>
                                            <xsd:enumeration
                                                value="ebuilliometry, recirculating still "/>
                                            <xsd:enumeration
                                                value="ebuilliometry, twin ebulliometer "/>
                                            <xsd:enumeration value="isochoric PVT apparatus"/>
                                            <xsd:enumeration value="isopiestic method"/>
                                            <xsd:enumeration value="isoteniscope"/>
                                            <xsd:enumeration value="Knudsen effusion method"/>
                                            <xsd:enumeration value="manometric method"/>
                                            <xsd:enumeration
                                                value="obtained from cryoscopic constant"/>
                                            <xsd:enumeration value="phase equilibration"/>
                                            <xsd:enumeration value="rate of evaporation"/>
                                            <xsd:enumeration value="spectrophotometry"/>
                                            <xsd:enumeration value="spectroscopy"/>
                                            <xsd:enumeration value="static method"/>
                                            <xsd:enumeration value="thermal analysis, Large sample"/>
                                            <xsd:enumeration value="transpiration method"/>
                                            <xsd:enumeration value="visual observation"/>
                                            <xsd:enumeration
                                                value="visual observation, stirred cell"/>
                                            <xsd:enumeration
                                                value="visual observation, unstirred cell"/>
                                            <xsd:enumeration
                                                value="visual observation, appearance of two phases"/>
                                            <xsd:enumeration
                                                value="visual observation, disappearance of two phases"/>
                                            <xsd:enumeration value="X-ray diffraction, ex-situ"/>
                                            <xsd:enumeration value="X-ray diffraction, in-situ"/>


                                        </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="otherMethod" type="xsd:string"/>
                            </xsd:choice>
                        </xsd:sequence>
                        <xsd:attribute name="transitionType" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>description of transition (eutectic, eutectoid, peritectic, peritectoid, monotectic, monotectoid, upper Consolute, consolute, triple point, quadruple/quintuple point, melting point, normal melting point, boiling point, normal boiling point, liquidus, solidus, LLE, SLE, solvus )</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="eutectic"/>
                                    <xsd:enumeration value="eutectoid"/>
                                    <xsd:enumeration value="peritectic"/>
                                    <xsd:enumeration value="peritectoid"/>
                                    <xsd:enumeration value="monotectic"/>
                                    <xsd:enumeration value="monotectoid"/>
                                    <xsd:enumeration value="upper consolute"/>
                                    <xsd:enumeration value="lower consolute"/>
                                    <xsd:enumeration value="melting point"/>
                                    <xsd:enumeration value="normal melting point"/>
                                    <xsd:enumeration value="boiling point"/>
                                    <xsd:enumeration value="normal boiling point"/>
                                    <xsd:enumeration value="triple point"/>
                                    <xsd:enumeration value="quadruple/qunituple point"/>
                                    <xsd:enumeration value="SLE"/>
                                    <xsd:enumeration value="LLE"/>
                                    <xsd:enumeration value="liquidus"/>
                                    <xsd:enumeration value="solidus"/>
                                    <xsd:enumeration value="solvus"/>
                                    <xsd:enumeration value="critical"/>
                                    <xsd:enumeration value="azeotrope"/>
                                    <xsd:enumeration value="vaporization"/>
                                    <xsd:enumeration value="sublimation"/>
                                    <xsd:enumeration value="fusion"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="refPhaseField" type="xsd:positiveInteger">
                            <xsd:annotation>
                                <xsd:documentation>reference for bulk composition (to phase field)</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="reaction">
                    <xsd:annotation>
                        <xsd:documentation>energies of reaction and constants pertaining to dissociation or equilibrium</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:choice>
                            <xsd:element name="dissociationConstant">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="baseDissociationConstant"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element name="acidDissociationConstant"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element name="waterDissociationConstant"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="naturalLogarithmOfWaterDissociationConstant"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="decimalLogarithmWaterDissociationConstant"
                                            type="tml:equilibriumConstantType"/>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="energy">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="molarEntropyOfReaction"
                                            type="tml:equibReactionEntropyType">
                                            <xsd:annotation>
                                                <xsd:documentation>entropy of a reaction per amount of substance (mole)
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarGibbsEnergyOfReaction"
                                            type="tml:equibReactionEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>Gibbs energy of a reaction per amount of substance (mole)
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molarEnthalpyOfReaction"
                                            type="tml:equibReactionEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>enthalpy of a reaction per amount of substance (mole)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element
                                            name="specificInternalEnergyOfReactionAtConstantVolume"
                                            type="tml:equibReactionEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>Internal energy of a reaction (at constant volume) divided by the combined mass of the reactants and products
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element
                                            name="molarInternalEnergyOfReactionAtConstantVolume"
                                            type="tml:equibReactionEnergyType">
                                            <xsd:annotation>
                                                <xsd:documentation>Internal energy of a reaction (at constant volume) per amount of substance (mole)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element
                                            name="potentialDifferenceOfAnElectrochemicalCell"
                                            type="tml:emfType"/>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="equilibriumConstant">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="thermodynamicEqulibriumConstant"
                                            type="tml:equilibriumConstantType">
                                            <xsd:annotation>
                                                <xsd:documentation>Quantity defined by K  = e   r G  / R T where  r G  is the standard reaction Gibbs energy, R the gas constant and T the thermodynamic temperature. Some chemists prefer the name thermodynamic equilibrium constant and the symbol K
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="equilibriumConstantInTermsOfMolality"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="equilibriumConstantInTermsOfAmountConcentration"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="equilibriumConstantInTermsOfPartialPressure"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element name="equilibriumConstantInTermsOfMoleFraction"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="naturalLogarithmOfThermodynamicEquilibriumConstant"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="naturalLogarithmOfThermodynamicEquilibriumConstantMolality"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="naturalLogarithmOfThermodynamicEquilibriumConstantAmountConcentration"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="naturalLogarithmOfThermodynamicEquilibriumConstantPartialPressure"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="naturalLogarithmOfThermodynamicEquilibriumConstantMoleFraction"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="decimalLogarithmThermodynamicEquilibriumConstant"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="decimalLogarithmThermodynamicEquilibriumConstantMolality"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="decimalLogarithmThermodynamicEquilibriumConstantAmountConcentration"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="decimalLogarithmThermodynamicEquilibriumConstantPartialPressure"
                                            type="tml:equilibriumConstantType"/>
                                        <xsd:element
                                            name="decimalLogarithmThermodynamicEquilibriumConstantMoleFraction"
                                            type="tml:equilibriumConstantType"/>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:choice>
                    </xsd:complexType>
                </xsd:element>
            </xsd:choice>
            <xsd:element minOccurs="0" name="xRef" type="tml:refDescriptionRefType">
                <xsd:annotation>
                    <xsd:documentation>link to reference information, used with dataRepresentations of difference, mean etc..</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0">
                <xsd:element name="CriticalEvaluation" type="tml:criticalEvaluationType"/>
                <xsd:element name="prediction" type="tml:predictionType"/>
            </xsd:choice>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="instrumentDetails"
                type="tml:instrumentType"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="scanningRange">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="xHigh">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="value" type="tml:numberOrNullType"/>
                                    <xsd:element name="unit">
                                        <xsd:annotation>
                                            <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:simpleType>
                                            <xsd:restriction base="xsd:string">
                                                <xsd:enumeration value="K"/>
                                                <xsd:enumeration value="Pa"/>
                                                <xsd:enumeration value="G (Gauss)"/>
                                            </xsd:restriction>
                                        </xsd:simpleType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="xLow">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="value" type="tml:numberOrNullType"/>
                                    <xsd:element name="unit">
                                        <xsd:annotation>
                                            <xsd:documentation>Standard method(s) of
                            measurement</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:simpleType>
                                            <xsd:restriction base="xsd:string">
                                                <xsd:enumeration value="K"/>
                                                <xsd:enumeration value="Pa"/>
                                                <xsd:enumeration value="G (Gauss)"/>
                                            </xsd:restriction>
                                        </xsd:simpleType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="dataRepresentation" use="required">
            <xsd:annotation>
                <xsd:documentation>description of how property data is represented (mean, difference, ratio, etc.)</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="Direct representation, x = x1"/>
                    <xsd:enumeration value="Difference with reference state, x =  x_1 - x_ref "/>
                    <xsd:enumeration
                        value="Mean between two conditions representation, x = ( x - x_ref)/ 2"/>
                    <xsd:enumeration
                        value="Ratio of difference with reference state, x = x1 / x_ref - 1 "/>
                    <xsd:enumeration value="Ratio with reference state, x = x_1 / x_ref "/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="id" type="xsd:integer">
            <xsd:annotation>
                <xsd:documentation>property id#, a unique identifier per data set, used for external linking in equation representation.  only useful for within this xml, non-transferable between xml documents.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="pointType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="label" type="xsd:string"/>
            <xsd:element name="phaseField" minOccurs="0" type="tml:phaseFieldRefType">
                <xsd:annotation>
                    <xsd:documentation>phase(s) of material for which the condition or property
                        describes</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="process" minOccurs="0" type="tml:systemRefType">
                <xsd:annotation>
                    <xsd:documentation>any additional processing that has occurred to the specimen
                        prior to testing for this point.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="var" type="tml:valueType" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>value and uncertainty</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="prop" type="tml:valueType" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>property and uncertainty</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="image" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:documentation>images associated with points or values</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="valueType">
        <xsd:sequence minOccurs="0">
            <xsd:element name="v" type="tml:numberOrNullType" minOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>value</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="unc" type="tml:uncertaintyType">
                <xsd:annotation>
                    <xsd:documentation>uncertainty.

Values and descriptive attributes as defined in the "Guide to the Expression of Uncertainty in Measurement." ISO-1993.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="instrumentType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="instruments">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="instrumentType" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>instrument name i.e. Scanning Electron
                                    Microscope, Potentiometer, Atomic Force
                                    Microscope</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="instrumentAcronym" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>instrument acronym commonly used or searched for
                                    i.e. SEM, TEM, AFM</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="model" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>instrument model or model
                                    number</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="brand" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>brand of instrument</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="customized" type="xsd:boolean">
                            <xsd:annotation>
                                <xsd:documentation>boolean to indicate whether the instrument has
                                    been modified.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="manufacturer" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>company that manufactures the
                                    instrument</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="otherDetails" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>string field for additional description of
                                    instrument, holders used, calibration details, purge gases,
                                    procedures used, third party attachments,
                                    etc.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="location">
                <xsd:annotation>
                    <xsd:documentation>location where the instrument resides</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="country" type="xsd:string"/>
                        <xsd:element minOccurs="0" name="region" type="xsd:string"/>
                        <xsd:element minOccurs="0" name="provence" type="xsd:string"/>
                        <xsd:element minOccurs="0" name="state" type="xsd:string"/>
                        <xsd:element minOccurs="0" name="city" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="organization" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>organization that used the instrument in the
                        study.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="organizationAcronym" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>


    <!-- ********************************************************-->
    <!--          Fixed/Variable condition definition            -->
    <!-- ********************************************************-->

    <xsd:complexType name="temperatureType">
        <xsd:annotation>
            <xsd:documentation>used for Critical temperature, lower consolute temperature,
                upper consolute temperature, cryoscopic constant mole fraction, LLE temperature, SLE
                temperature, eutectic temperature, monotectic temperature, normal melting
                temperature, peritectic temperature, triple point temperature, azeotrope
                temperature, boiling temperature, normal boiling temperature, temperature of half
                conversion, peak temperature, zero Gibbs energy temperature, </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="K">
                <xsd:annotation>
                    <xsd:documentation>measured unit of variable/property in SI</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="K"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="scale" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>temperature scale</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="ITS-90"/>
                        <xsd:enumeration value="EPT-76"/>
                        <xsd:enumeration value="IPTS-68"/>
                        <xsd:enumeration value="IPTS-48"/>
                        <xsd:enumeration value="ITS-27"/>
                        <xsd:enumeration value="NBS-55"/>
                        <xsd:enumeration value="He3-62"/>
                        <xsd:enumeration value="He4-62"/>
                        <xsd:enumeration value="He4-58"/>
                        <xsd:enumeration value="He4-55E"/>
                        <xsd:enumeration value="He4-49"/>
                        <xsd:enumeration value="thermodynamic"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Note: if method is determined by radiance, it represents
                        radiance temperature, if wavelength is reported in table, it is the
                        wavelength used during pyrometry.</xsd:documentation>
                </xsd:annotation>
                <xsd:element name="analyticalInstrument">
                    <xsd:annotation>
                        <xsd:documentation>analytical instrument used for measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="determiend by radiance (Radiance Temperature)"/>
                            <xsd:enumeration value="platinum resistance thermometer"/>
                            <xsd:enumeration value="pyrometer"/>
                            <xsd:enumeration value="pyrometer, optical"/>
                            <xsd:enumeration value="pyrometer, infrared"/>
                            <xsd:enumeration value="pyrometer, disappearing filament"/>
                            <xsd:enumeration value="thermistor"/>
                            <xsd:enumeration value="thermocouple"/>
                            <xsd:enumeration value="thermocouple J -Fe/Constantan"/>
                            <xsd:enumeration value="thermocouple K -Chromel/Alumel"/>
                            <xsd:enumeration value="thermocouple E -Chromel/Constantan"/>
                            <xsd:enumeration value="thermocouple T -Cu/Constantin"/>
                            <xsd:enumeration value="thermocouple N -Nicrosil/Nisil"/>
                            <xsd:enumeration value="thermocouple B -Pt/Pt-30%Rh"/>
                            <xsd:enumeration value="thermocouple S -Pt/Pt-10%Rh"/>
                            <xsd:enumeration value="thermocouple R -Pt/Pt-13%Rh"/>
                            <xsd:enumeration value="thermocouple U -Cu/Cu-Ni"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherAnalyticalInstrument" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a analytical instrument that is not listed in ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="compositionType">
        <!-- Restrict units to composition units, add relative to concepts for solutions a ratio measurements -->
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0" default="dimensionless"/>
            <xsd:element name="component" minOccurs="1" maxOccurs="1" type="tml:chemicalRefType">
                <xsd:annotation>
                    <xsd:documentation>component measured</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="units">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <!--unsure if this should be dimensionless or wt/at percent/fraction-->
                        <xsd:enumeration value="atomic fraction"/>
                        <xsd:enumeration value="atomic percent"/>
                        <xsd:enumeration value="weight fraction"/>
                        <xsd:enumeration value="weight percent"/>
                        <xsd:enumeration value="Molality, mol/kg"/>
                        <xsd:enumeration
                            value="Ratio of amount of solute to mass of solution, mol/kg"/>
                        <xsd:enumeration
                            value="Ratio of mass of solute to volume of solution, kg/m3"/>
                        <xsd:enumeration value="Amount ratio of solute to solvent"/>
                        <xsd:enumeration value="Mass ratio of solute to solvent"/>
                        <xsd:enumeration value="Volume ratio of solute to solvent"/>
                        <xsd:enumeration value="Initial mole fraction of solute"/>
                        <xsd:enumeration value="Final mole fraction of solute"/>
                        <xsd:enumeration value="Initial mass fraction of solute"/>
                        <xsd:enumeration value="Final mass fraction of solute"/>
                        <xsd:enumeration value="Initial molality of solute, mol/kg"/>
                        <xsd:enumeration value="Final molality of solute, mol/kg"/>
                        <xsd:enumeration value="amount of atoms"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice>
                <xsd:element name="phase" minOccurs="1" type="tml:phaseRefType">
                    <xsd:annotation>
                        <xsd:documentation>phase to which the measurand was conducted
                            on</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="nominalOrBulkComposition" type="xsd:boolean" default="true">
                    <xsd:annotation>
                        <xsd:documentation>boolean to indicate *assumed* composition (i.e. mixed a
                            large amount and assumed homogenous content) or *measured* from multiple phases (i.e. a larger area chemical analysis)</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>method used for measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Mass spectroscopy"/>
                            <xsd:enumeration value="Energy dispersive spectroscopy"/>
                            <xsd:enumeration value="Electron probe microganalysis"/>
                            <xsd:enumeration value="Secondary ion mass spectroscopy"/>
                            <xsd:enumeration value="Wave dispersive spectroscopy"/>
                            <xsd:enumeration value="Optical Emission Spectroscopy"/>
                            <xsd:enumeration value="Spark Emission Spectroscopy"/>
                            <xsd:enumeration value="Electron energy loss spectroscopy"/>
                            <xsd:enumeration value="X-ray photoelectron spectroscopy"/>
                            <xsd:enumeration value="X-ray fluorescence"/>
                            <xsd:enumeration value="Gas chromatography"/>
                            <xsd:enumeration value="Chemical analysis"/>
                            <xsd:enumeration value="NMR"/>
                            <xsd:enumeration value="Acid-base titration"/>
                            <xsd:enumeration value="other type of titration"/>
                            <xsd:enumeration value="Atom probe tomography"/>
                            <xsd:enumeration value="Dissolution/Filtration experiment"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>input for string-text of a method that is not listed in ThermoML</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="magneticFieldDensityType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="Tesla">
                <xsd:annotation>
                    <xsd:documentation>units associated with of measurand</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Tesla"/>
                    </xsd:restriction>
                </xsd:simpleType>
