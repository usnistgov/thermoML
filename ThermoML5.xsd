    <!--used for the description of a phase field -->
    <xsd:complexType name="phaseFieldType">
        <xsd:sequence>
            <xsd:annotation>
                <xsd:documentation>list of n>1 phases present</xsd:documentation>
            </xsd:annotation>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="phase" minOccurs="1" maxOccurs="unbounded" type="tml:phaseRefType">
                <xsd:annotation>
                    <xsd:documentation>link to phase(s)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="zeroPhaseFraction" maxOccurs="unbounded"
                type="tml:phaseRefType">
                <xsd:annotation>
                    <xsd:documentation>for boundaries--list of phase(s) with fraction
                        decreasing/increasing from zero</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="criticalPoint" type="xsd:boolean" default="false">
                <xsd:annotation>
                    <xsd:documentation>indicator of critical point (mag. transition, dew point,
                        melting point, etc.)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:positiveInteger" use="required">
            <xsd:annotation>
                <xsd:documentation>region id#, where a region is defined as (the phases present). Only useful for within this XML, non-transferable between XML documents.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <!-- ***********************************************************-->
    <!--          Dataset and dataset component definitions         -->
    <!-- ***********************************************************-->

    <xsd:complexType name="datasetType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="citation">
                <xsd:annotation>
                    <xsd:documentation>link for citation info</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="tml:citationRefType">
                            <xsd:sequence minOccurs="0">
                                <xsd:element minOccurs="0" name="label" type="xsd:string"/>
                            </xsd:sequence>
                        </xsd:extension>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="system">
                <xsd:annotation>
                    <xsd:documentation>link to system (sample-processing-components and reaction info)</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="tml:systemRefType">
                            <xsd:sequence minOccurs="0">
                                <xsd:element minOccurs="0" name="label" type="xsd:string"/>
                            </xsd:sequence>
                        </xsd:extension>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="referenceDetails" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>ref keys for descriptors for reference, initial, or final states.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="label" type="xsd:string"/>
                        <xsd:element maxOccurs="unbounded" name="reference" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="label" type="xsd:string" minOccurs="0"/>
                                    <xsd:element name="refComponent" minOccurs="0" maxOccurs="1"
                                        type="tml:chemicalRefType">
                                        <xsd:annotation>
                                            <xsd:documentation>link for component linked to condition (used
                                                for reference to another component, i.e. compared to
                                                helium or alumina) </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="refPhase" minOccurs="0"
                                        type="tml:phaseRefType">
                                        <xsd:annotation>
                                            <xsd:documentation>link for phase or phaseField for reference</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="refProcess" minOccurs="0"
                                        type="tml:systemRefType">
                                        <xsd:annotation>
                                            <xsd:documentation>link for any processing that has been done
                                                between where purity was described and the condition
                                                at which is represented</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="refSystem"
                                        type="tml:systemRefType">
                                        <xsd:annotation>
                                            <xsd:documentation>link for system (component-processing-sample and reactions)</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="standardStatePureCompound" type="xsd:boolean"
                            minOccurs="0"/>
                        <xsd:element minOccurs="0" name="infiniteDillutionSolute" type="xsd:boolean"
                        />
                    </xsd:sequence>
                    <xsd:attribute name="id" type="xsd:positiveInteger" use="required">
                        <xsd:annotation>
                            <xsd:documentation>referenceDetails id#, (i.e. reference state conditions) only useful for within this XML, non-transferable between XML documents.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="fixedConditions" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Condition Info (T,P, etc.) in header of
                        table</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence minOccurs="1">
                        <xsd:element minOccurs="0" name="label" type="xsd:string"/>
                        <xsd:element name="condition" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>conditions, method of determination information, unit information for value</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:complexContent>
                                    <xsd:extension base="tml:conditionType">
                                        <xsd:sequence>
                                            <xsd:element name="var" type="tml:valueType"
                                                maxOccurs="1">
                                                <xsd:annotation>
                                                  <xsd:documentation>variable value for the selected
                                    condition</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                        </xsd:sequence>
                                    </xsd:extension>
                                </xsd:complexContent>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="crucibleMaterial" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Container or crucible material used during
                                    experiment. (material in contact with
                                    specimens)</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="Al2O3"/>
                                    <xsd:enumeration value="aluminum"/>
                                    <xsd:enumeration value="BeO"/>
                                    <xsd:enumeration value="BN"/>
                                    <xsd:enumeration value="brass"/>
                                    <xsd:enumeration value="copper"/>
                                    <xsd:enumeration value="graphite"/>
                                    <xsd:enumeration value="iron"/>
                                    <xsd:enumeration value="MgO"/>
                                    <xsd:enumeration value="molybdenum"/>
                                    <xsd:enumeration value="platinum"/>
                                    <xsd:enumeration value="Pt/Rh"/>
                                    <xsd:enumeration value="SiC"/>
                                    <xsd:enumeration value="Si3N4"/>
                                    <xsd:enumeration value="silica"/>
                                    <xsd:enumeration value="silver"/>
                                    <xsd:enumeration value="spinel"/>
                                    <xsd:enumeration value="stainless_steel"/>
                                    <xsd:enumeration value="tantalum"/>
                                    <xsd:enumeration value="ThO2"/>
                                    <xsd:enumeration value="TiO2"/>
                                    <xsd:enumeration value="tungsten"/>
                                    <xsd:enumeration value="W/Re"/>
                                    <xsd:enumeration value="Y2O3"/>
                                    <xsd:enumeration value="YSZ"/>
                                    <xsd:enumeration value="zinc"/>
                                    <xsd:enumeration value="ZrO2"/>
                                    <xsd:enumeration value="ZrO2/Zr"/>
                                    <xsd:enumeration value="None"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="atmosphere" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Atmosphere in contact with specimen during
                                    experiment</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="vacuum"/>
                                    <xsd:enumeration value="air"/>
                                    <xsd:enumeration value="argon"/>
                                    <xsd:enumeration value="helium"/>
                                    <xsd:enumeration value="nitrogen"/>
                                    <xsd:enumeration value="Ar/H2"/>
                                    <xsd:enumeration value="Ar/He"/>
                                    <xsd:enumeration value="He/H2"/>
                                    <xsd:enumeration value="N2/H2"/>
                                    <xsd:enumeration value="water"/>
                                    <xsd:enumeration value="salts"/>
                                    <xsd:enumeration value="CO2"/>
                                    <xsd:enumeration value="hydrogen"/>
                                    <xsd:enumeration value="oil"/>
                                    <xsd:enumeration value="oxygen"/>
                                    <xsd:enumeration value="lead"/>
                                    <xsd:enumeration value="kerosene"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="variableConditions" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Condition values (T,P, etc.) found as column(s) in
                        table</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="label" type="xsd:string"/>
                        <xsd:element maxOccurs="unbounded" name="condition" type="tml:conditionType">
                            <xsd:annotation>
                                <xsd:documentation>conditions, method of determination information, unit information for value</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="property" minOccurs="1" maxOccurs="unbounded"
                type="tml:propertyType2">
                <xsd:annotation>
                    <xsd:documentation>Property information found in column(s) of
                        table</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="equation" maxOccurs="unbounded" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>for the representation of mathematical equations. Useful for matML extensions</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="label" type="xsd:string"/>
                        <xsd:sequence maxOccurs="unbounded">
                            <xsd:choice>
                                <xsd:element name="eqName">
                                    <xsd:annotation>
                                        <xsd:documentation>thermoML list of standard equation names</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                        <xsd:restriction base="xsd:string">
                                            <xsd:enumeration value="ThermoML.Antoine.VaporPressure"/>
                                            <xsd:enumeration value="ThermoML.CustomExpansion"/>
                                            <xsd:enumeration value="ThermoML.Helmholtz3General.EOS"/>
                                            <xsd:enumeration value="ThermoML.Helmholtz4General.EOS"/>
                                            <xsd:enumeration
                                                value="ThermoML.WagnerLinear.VaporPressure"/>
                                            <xsd:enumeration
                                                value="ThermoML.Wagner25Linear.VaporPressure"/>
                                            <xsd:enumeration
                                                value="ThermoML.Wagner36Linear.VaporPressure"/>
                                            <xsd:enumeration value="ThermoML.PolynomialExpansion"/>
                                            <xsd:enumeration
                                                value="ThermoML.SpanWagner12Nonpolar.EOS"/>
                                            <xsd:enumeration value="ThermoML.SpanWagner12Polar.EOS"
                                            />
                                        </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:element>
                                <xsd:element name="otherEquationName" type="xsd:string">
                                    <xsd:annotation>
                                        <xsd:documentation>for equation names not listed in thermoML library.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:choice>
                            <xsd:element name="mathSource" type="xsd:anyURI">
                                <xsd:annotation>
                                    <xsd:documentation>link to the location of where teh XML representation of the specified equation is stored</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="equationProperty">
                            <xsd:annotation>
                                <xsd:documentation>defines properties in equation</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:complexContent>
                                    <xsd:extension base="tml:equationComponentType">
                                        <xsd:sequence>
                                            <xsd:element name="propertyRef">
                                                <xsd:annotation>
                                                  <xsd:documentation>keyref to property id#.  ThermoML4.0 was nPropNumber
</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                  <xsd:attribute name="ref"
                                                  type="xsd:positiveInteger"/>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </xsd:sequence>
                                    </xsd:extension>
                                </xsd:complexContent>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="equationCondition">
                            <xsd:annotation>
                                <xsd:documentation>defined as an immediate subelement of the property eqProperty block, not necessarily a constraint for the equation.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:complexContent>
                                    <xsd:extension base="tml:equationComponentType">
                                        <xsd:sequence>
                                            <xsd:element name="conditionRef">
                                                <xsd:annotation>
                                                  <xsd:documentation>keyref to constraint condition id#.  ThermoML4.0 was nPropNumber
</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                  <xsd:attribute name="ref"
                                                  type="xsd:positiveInteger"/>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </xsd:sequence>
                                    </xsd:extension>
                                </xsd:complexContent>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="equationVariable">
                            <xsd:annotation>
                                <xsd:documentation>defines variables in equation</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:complexContent>
                                    <xsd:extension base="tml:equationComponentType">
                                        <xsd:sequence>
                                            <xsd:element name="conditionRef">
                                                <xsd:annotation>
                                                  <xsd:documentation>keyref to condition id#.  ThermoML4.0 was nPropNumber
</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                  <xsd:attribute name="ref"/>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </xsd:sequence>
                                    </xsd:extension>
                                </xsd:complexContent>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="equationParameter">
                            <xsd:annotation>
                                <xsd:documentation>associates the parameter to a particular row and column in the covariance matrix</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element minOccurs="0" name="parameterNumber"
                                        type="xsd:integer">
                                        <xsd:annotation>
                                            <xsd:documentation>associates the parameter to a particular row and column in the covariance matrix. Parameters without this values do not contribute to the covariance, if it is established independent of the represented property values</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:sequence maxOccurs="unbounded">
                                        <xsd:element name="symbol" type="xsd:string">
                                            <xsd:annotation>
                                                <xsd:documentation>Used to map a symbol used in the equation definition (a ThermoMLEquation file) to a particular property defined in the ThermoML file. ThermoML 4.0 was sEqParSymbol</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element maxOccurs="unbounded" minOccurs="0"
                                            name="index" type="xsd:integer">
                                            <xsd:annotation>
                                                <xsd:documentation>for matrix representations.  used to map the property to a particular index in the equation definition. One index is used for a vector of parameters and two indexes for a matrix element (e.g., binary interaction parameters g represented
as gij have indexes i and j).   ThermoML 4.0 was nEqParIndex
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                    <xsd:element name="parameterValue" type="xsd:float">
                                        <xsd:annotation>
                                            <xsd:documentation>stores the numerical value of the parameter</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="parameterValueDigits" type="xsd:integer">
                                        <xsd:annotation>
                                            <xsd:documentation>stores the total number of digits in the paramterValue</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="equationConstant">
                            <xsd:annotation>
                                <xsd:documentation>associates constants to a particular row and column in the equation</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="constantSymbol" type="xsd:string">
                                        <xsd:annotation>
                                            <xsd:documentation>Used to map a symbol used in the equation definition (a ThermoMLEquation file) to a particular property defined in the ThermoML file. ThermoML 4.0 was sEqConstantSymbol</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element maxOccurs="unbounded" minOccurs="0"
                                        name="constantIndex" type="xsd:integer">
                                        <xsd:annotation>
                                            <xsd:documentation>for matrix representations.  used to map the property to a particular index in the equation definition. One index is used for a vector of parameters and two indexes for a matrix element (e.g., binary interaction parameters g represented
as gij have indexes i and j).   ThermoML 4.0 was nEqConstantIndex
</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="constantValue" type="xsd:float">
                                        <xsd:annotation>
                                            <xsd:documentation>stores the numerical value of the parameter</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="consantValueDigits" type="xsd:integer">
                                        <xsd:annotation>
                                            <xsd:documentation>stores the total number of digits in the paramterValue</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="covariance">
                            <xsd:annotation>
                                <xsd:documentation>used to store the elements of the covariance matrix.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="parameterOne" type="xsd:integer">
                                        <xsd:annotation>
                                            <xsd:documentation>used to specify a particular equation parameter</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="parameterTwo" type="xsd:integer">
                                        <xsd:annotation>
                                            <xsd:documentation>used to specify a second parameter defined within parameterOne</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="convarianceValue" type="xsd:double">
                                        <xsd:annotation>
                                            <xsd:documentation>stores the covariance value for the two defined parameters.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="nCovarianceLevOfConfid" type="xsd:float">
                            <xsd:annotation>
                                <xsd:documentation>stores the level of confidence (in percent) associated with uncertainties calculated with the covariance values</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="point" type="tml:pointType" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>information stored in row(s) in a table</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="expPurpose" minOccurs="0" default="Principal objective of the work">
                <xsd:annotation>
                    <xsd:documentation>purpose of measurement i.e. primary purpose, for
                        identification of substance, etc.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:annotation>
                        <xsd:documentation>Purpose of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Principal objective of the work"/>
                        <xsd:enumeration value="Secondary purpose (by-product of other objective)"/>
                        <xsd:enumeration
                            value="Determined for identification of a synthesized compound"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="compiler" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>the name of the person who compiled the data contained in the
ThermoML file</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="contributor" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>an identifier for a particular project, institution, or general
source of the ThermoML file</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="date" minOccurs="0" type="xsd:date">
                <xsd:annotation>
                    <xsd:documentation>date of ThermoML file creation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:integer">
            <xsd:annotation>
                <xsd:documentation>datasetid#--a unique identifier per data set, used for external linking in equation representation.  only useful for within this xml, non-transferable between xml documents.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="microstructuralEvalType">
        <xsd:sequence>
            <xsd:element name="value" type="tml:numberOrNullType"/>
            <xsd:element name="unit">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="average diameter in m"/>
                        <xsd:enumeration value="ASTM No."/>
                        <xsd:enumeration value="grains per square m"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice>
                <xsd:element name="method" minOccurs="1">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="EBSD characterization"/>
                            <xsd:enumeration value="Linear Intercept Method"/>
                            <xsd:enumeration value="Grain Boundary Intersection Count"/>
                            <xsd:enumeration value="Grain Intercept Count"/>
                            <xsd:enumeration value="Planimetric Method"/>
                            <xsd:enumeration value="Estimation by author"/>
                            <xsd:enumeration value="stated by supplier"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element minOccurs="1" name="otherMethod" type="xsd:string"/>
            </xsd:choice>
            <xsd:element minOccurs="0" maxOccurs="1" name="uncertainty" type="tml:uncertaintyType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="conditionType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="label" type="xsd:string"/>
            <xsd:element name="refState" minOccurs="0" type="tml:refDescriptionRefType">
                <xsd:annotation>
                    <xsd:documentation>link to reference state: component, system, phase, process etc.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice>
                <xsd:element name="amountDensity" type="tml:amountConcentrationType"/>
                <xsd:element name="composition" type="tml:compositionType"/>
                <!--added  variables-->
                <xsd:element name="enthalpy" type="tml:enthalpyType"/>
                <xsd:element name="frequency" type="tml:frequencyType"/>
                <xsd:element name="heatingRate" type="tml:heatingRateType"/>
                <xsd:element name="ionicStrength" type="tml:ionicStrengthType"/>
                <xsd:element name="magneticFieldDensity" type="tml:magneticFieldDensityType"/>
                <xsd:element name="magneticFieldStrength" type="tml:magneticFieldStrengthType"/>
                <xsd:element name="massDensity" type="tml:massDensityType"/>
                <xsd:element name="molality" type="tml:molalityType"/>
                <xsd:element name="molarDensity" type="tml:molarDensityType"/>
                <xsd:element name="molarVolume" type="tml:molarVolumeType"/>
                <xsd:element name="phaseVolumeFraction" type="tml:phaseVolumeFractionType"> </xsd:element>
                <xsd:element name="pH" type="tml:phType"/>
                <xsd:element name="pC" type="tml:pCType"/>
                <xsd:element name="pressure" type="tml:pressureType"/>
                <xsd:element name="specificVolume" type="tml:specificVolumeType"/>
                <xsd:element name="temperature" type="tml:temperatureType"/>
                <xsd:element name="waveLength" type="tml:waveLengthType"/>
            </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="conditionDescription" use="required">
            <xsd:annotation>
                <xsd:documentation>condition description: equilibrium, reference, high avg., low avg.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="reference condition"/>
                    <xsd:enumeration value="equilibrium condition"/>
                    <xsd:enumeration value="high condition value of average"/>
                    <xsd:enumeration value="low condition value of average"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="id" type="xsd:integer">
            <xsd:annotation>
                <xsd:documentation>condition id#, a unique identifier per data set, used for external linking in equation representation.  only useful for within this xml, non-transferable between xml documents.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="propertyType2">
        <xsd:annotation>
            <xsd:documentation>Reorganized property list
--annotated
--depricated phase transition conditions
--methods updated</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="label" type="xsd:string"/>
            <xsd:choice>
                <xsd:element name="thermodynamic">
                    <xsd:complexType>
                        <xsd:choice>
                            <xsd:element name="activitiy">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="relativeActivity" type="tml:activityType">
                                            <xsd:annotation>
                                                <xsd:documentation>A factor in relation between chemical potential and composition of a mixture or solution. Commonly referred to as activity
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="activityCoefficient"
                                            type="tml:activityType">
                                            <xsd:annotation>
                                                <xsd:documentation>a factor used in thermodynamics to account for deviations from ideal behavior in a mixture of chemical substances.
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="fugacity" type="tml:partialPressureType">
                                            <xsd:annotation>
                                                <xsd:documentation>fugacity of a substance/component B, f B or p B , in a gaseous mixture is defined by f B =  B lim p  0 p  B  B, where p B is the partial pressure of B and  B its absolute activity</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="fugacityCoefficient"
                                            type="tml:fugacityCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>fugacity divided by the  partial pressure of a gaseous constituent</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="meanIonicActivity"
                                            type="tml:ionicActivityType">
                                            <xsd:annotation>
                                                <xsd:documentation>The product of the molalities of the cations or the anions normalized by the standard molar solution(m^theta = 1 mole/kg). a_= m_gamma_/m^theta.  Note that for an individual ion, neither the activity , nor the activity coefficient is experimentally measurable</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="meanIonicActivityCoefficient"
                                            type="tml:ionicActivityCoefficientType">
                                            <xsd:annotation>
                                                <xsd:documentation>the ratio of the activities (gamma_) for the cations (gamma_+) and anions(gamma_-) raised to their respective numbers per formula unit of an electrolyte (A_mu_+B_mu_-) in a solution. 
gamma_ = = gamma_+^(mu_+)+gamma_-^(mu_-)	Note that for an individual ion, neither the activity , nor the activity coefficient is experimentally measurable</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="electroChemicalCellPotential"
                                            type="tml:emfType">
                                            <xsd:annotation>
                                                <xsd:documentation>the measured cell potential of a solution, which is related to the partial molar Gibbs energy of the substance in a specified phase</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="composition">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="amountConcentration"
                                            type="tml:amountConcentrationType">
                                            <!-- example unit: mol/m^3 -->
                                            <xsd:annotation>
                                                <xsd:documentation>Amount of a constituent divided by the volume of the mixture. Also called amount-of-substance concentration, substance concentration (in clinical chemistry) and in older literature molarity. For entities B it is often denoted by  B. The common unit is mole per cubic decimeter (mole dm 3) or mole per litre(mole L 1) sometimes denoted by M. Commonly called molarity of solution</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="amountRatioOfSoluteToSolvent"
                                            type="tml:compositionRatioType">
                                            <!-- example unit: dimensionless?  -->
                                            <xsd:annotation>
                                                <xsd:documentation>equilibrium ratio of solute to solvent (mole/mole)</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="massConcentration"
                                            type="tml:massPerVolumeSolutionType">
                                            <!-- example unit: kg/m^3  -->
                                            <xsd:annotation>
                                                <xsd:documentation>Mass of a constituent divided by the volume of the mixture</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="massFraction" type="tml:massFractionType">
                                            <xsd:annotation>
                                                <xsd:documentation>mass of a constituent of a mixture divided by the sum of mass of all constituents prior to mixing</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="massRatioOfSoluteToSolvent"
                                            type="tml:massFractionType">
                                            <xsd:annotation>
                                                <xsd:documentation>Ratio of mass of solute to mass of a solution
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="molality" type="tml:molalityType">
                                            <xsd:annotation>
                                                <xsd:documentation>Amount of entities of a solute divided by the mass of the solvent
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="moleFraction" type="tml:moleFractionType">
                                            <xsd:annotation>
                                                <xsd:documentation>Amount of a constituent divided by the total amount of all constituents in the mixture. It is also called mole fraction. Amount fraction is equal to the number fraction: the number of entities of one constituent divided by the total number of entities in the mixture</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="ratioOfAmountOfSoluteToMassOfSolution"
                                            type="tml:molalityType">
                                            <xsd:annotation>
                                                <xsd:documentation>Ratio of amount of a solute (moles) to mass of a solution
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="volumeFraction"
                                            type="tml:volumeFractionType">
                                            <xsd:annotation>
                                                <xsd:documentation>Volume of a constituent of a mixture divided by the sum of volumes of all constituents prior to mixing</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="volumeRatioOfSoluteToSolvent"
                                            type="tml:volumeRatioOfSoluteToSolventType">
                                            <!-- example unit: m^3/m^3  -->
                                            <xsd:annotation>
                                                <xsd:documentation>Ratio of volume of solute to solvent. Ratio of partial pressure to molarity</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="compressibility">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="adiabaticCompressibility"
                                            type="tml:compressibilityType">
                                            <xsd:annotation>
                                                <xsd:documentation>The measure of change volume of matter changes when placed under pressure in a thermally insulated system.  kappa=1/V (dV/dP)_S   
</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="compressibilityFactor"
                                            type="tml:compressibilityFactorType">
                                            <xsd:annotation>
                                                <xsd:documentation>Product of pressure and molar volume divided by the gas constant and thermodynamic temperature.  Also known as compression factor</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessAdiabaticCompressibility"
                                            type="tml:compressibilityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference between real/measured and ideal adiabatic compressibility</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="excessIsothermalCompressibility"
                                            type="tml:compressibilityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the difference between real/measured and ideal isothermal compressibility</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="isothermalCompressibility"
                                            type="tml:compressibilityType">
                                            <xsd:annotation>
                                                <xsd:documentation>the measure of how much a volume of matter changes when placed under pressure at constant temperature</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="cryoscopicConstant">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="moleFractionCryoscopicConstant">
                                            <xsd:annotation>
                                                <xsd:documentation>A constant (A) used in describing the mole fraction solute (x) that forces depression or increase of freezing point from the pure freezing point (Del t) of a solution. Del t = x/A
</xsd:documentation>
                                            </xsd:annotation>
