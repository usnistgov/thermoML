            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/enthalpy/excessMolarEnthlapyMolarEnthlpyOfMixing/solute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-excessMolarEnthlpyMolarEnthlapyOfMixingSolvent-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/enthalpy/excessMolarEnthlapyMolarEnthlpyOfMixing/solvent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-molarEnthalpyOfDilutionSolute-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/enthalpy/molarEnthalpyOfDilution/solute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-molarEnthalpyOfDilutionSolvent-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/enthalpy/molarEnthalpyOfDilution/solvent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-molarEnthalpyOfMixingSolute-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/enthalpy/molarEnthlalpyOfMixingWithSolvent/solute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-molarEnthalpyofMixingSolvent-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/enthalpy/molarEnthlalpyOfMixingWithSolvent/solvent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-molarEnthalpyOfSolutionSolute-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
