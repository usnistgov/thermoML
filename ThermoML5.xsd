<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema version="5.0.0" targetNamespace="http://www.iupac.org/namespaces/ThermoML"
    xmlns:tml="http://www.iupac.org/namespaces/ThermoML"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xsd:annotation>
        <xsd:documentation>ThermoML version 5.0</xsd:documentation>
    </xsd:annotation>

    <!-- ********** -->
    <!--Root element-->
    <!-- ********** -->

    <xsd:element name="dataReport" type="tml:dataReportType">

        <!-- ***************************************-->
        <!--     id keys for key-key references     -->
        <!-- ***************************************-->

        <xsd:key name="citationIDKey">
            <xsd:annotation>
                <xsd:documentation>Key for bibliographic content</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="citation"/>
            <xsd:field xpath="@id"/>
        </xsd:key>
        <xsd:key name="chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>Key for chemical content</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="chemical"/>
            <xsd:field xpath="@id"/>
        </xsd:key>
        <xsd:key name="phaseIDKey">
            <xsd:annotation>
                <xsd:documentation>Key for phase characerization content</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="phase"/>
            <xsd:field xpath="@id"/>
        </xsd:key>
        <xsd:key name="phaseFieldIDKey">
            <xsd:annotation>
                <xsd:documentation>Key for phases present (i.e. phase field characerization content)</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="phase"/>
            <xsd:field xpath="@id"/>
        </xsd:key>
        <xsd:key name="systemIDKey">
            <xsd:annotation>
                <xsd:documentation>Key for physical system characterization
                    content</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="system"/>
            <xsd:field xpath="@id"/>
        </xsd:key>
        <xsd:key name="referenceDetailIDKey">
            <xsd:annotation>
                <xsd:documentation>Key for reference condition description</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataset"/>
            <xsd:field xpath="@id"/>
        </xsd:key>
        <xsd:key name="equationDataSetIDKey">
            <xsd:annotation>
                <xsd:documentation>Key for pure or mixture data block equation representation dataset description</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataset"/>
            <xsd:field xpath="@id"/>
        </xsd:key>
        <xsd:key name="reactionDataSetIDKey">
            <xsd:annotation>
                <xsd:documentation>Key for pure or mixture data block equation representation dataset description</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataset"/>
            <xsd:field xpath="@id"/>
        </xsd:key>
        <xsd:key name="conditionIDKey">
            <xsd:annotation>
                <xsd:documentation>Key for equations with fixed condition descriptions</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataset"/>
            <xsd:field xpath="@id"/>
        </xsd:key>
        <xsd:key name="varConditionIDKey">
            <xsd:annotation>
                <xsd:documentation>Key for equations with variable condtion descriptions</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataset"/>
            <xsd:field xpath="@id"/>
        </xsd:key>
        <xsd:key name="propertyIDKey">
            <xsd:annotation>
                <xsd:documentation>Key for property ids related to data point or equation description</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataset"/>
            <xsd:field xpath="@id"/>
        </xsd:key>

        <!-- ******************************** -->
        <!--     citation key references      -->
        <!-- ******************************** -->

        <xsd:keyref name="citation-translationOf-citationKeyRef" refer="tml:citationIDKey">
            <xsd:annotation>
                <xsd:documentation>reference to citaion id to identify origin document of translation</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/citation/translationOf"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="citation-erratumOf-citationKeyRef" refer="tml:citationIDKey">
            <xsd:annotation>
                <xsd:documentation>reference to citation id to identify origin document for erratum</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/citation/erratumOf"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-other-citationKeyRef" refer="tml:citationIDKey">
            <xsd:annotation>
                <xsd:documentation>reference to identify citation information for dataset</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/citation"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-prediction-citationKeyRef" refer="tml:citationIDKey">
            <xsd:annotation>
                <xsd:documentation>reference to citation infromation used with prediction</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/property/prediction/citation"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-criticalEvaluation-citationKeyRef" refer="tml:citationIDKey">
            <xsd:annotation>
                <xsd:documentation>reference to citation infromation used with prediction</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/property/criticalEvaluation/citation"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>

        <!-- *********************************-->
        <!--       chemical key references    -->
        <!-- *********************************-->

        <xsd:keyref name="phase-molecularFormula-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>Key reference to chemical components of molecular formula</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/phase/molecularFormula/component/chemical"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-multiComponent-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>Key reference to a chemical species in a multicomponent mixture</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/system/multiComponent/component/chemical"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-processing-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>Key reference to a prior system, or processing history of a system</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/system/processing/component/chemical"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-processingPressure-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to use with field when partial pressure is used</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/system/processing/processingStep/pressure/info/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-reactionParticipant-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>Key reference for common identification of participant in chemical reaciton</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/system/reaction/participant/chemical"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-reactionSolvent-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>Key reference for common identification of solvent in chemical reaciton</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/system/reaction/solvent/chemical"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-reactionAuxillarySubstance-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>Key reference for common identification of auxiliary substance in chemical reaciton</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/system/reaction/auxiliarySubstance/chemical"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-sample-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>Key reference for uncharacterized chemicals in a specimen</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/system/sample/chemical"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="system-sampleCharacterization-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>Key reference for a characterized chemical component identified in a sample</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/system/sample/componentCharacterization/chemical"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-reference-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference for chemical component of reference state</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/referenceDetails/reference/refComponent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-fixedPressure-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference for chemical id of partial pressure components of the atmosphere</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/fixedConditions/condition/pressure/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-fixedComposition-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component of a composition</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/fixedConditions/condition/composition/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-variablePressure-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference for chemical id of partial pressure components of the atmosphere</xsd:documentation>
            </xsd:annotation>
            <xsd:selector xpath="dataReport/dataset/variableConditions/condition/pressure/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-variableComposition-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component of a composition</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/variableConditions/condition/composition/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-relativeActivity-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component of a relative activity</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/activity/relativeActivity/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-activityCoefficient-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the chamical component of the activity coefficient</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/activity/activityCoefficient/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-fugacityComponent-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the chemical component of the fugacity</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/activity/fugacity/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-fugacityOtherComponents-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe other chemical components in the atmosphere.</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/activity/fugacity/gaseousComponents/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-fugacityCoefficient-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe component linked to fugacity</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/activity/fugacityCoefficient/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-fugacityCoefficientOtherComponents-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe components linked to gaseous mixture </xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/activity/fugacityCoefficient/gaseousComponent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-meainIonicActivity-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component measured for meain ionic activity</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/activity/meanIonicActivity/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-meainIonicActivityCoefficient-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the componetn measured for the meain ionic activity coefficeint</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/activity/meanIonicActivityCoefficient/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-amountConcentration-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the amountn concentraiton component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/amountConcentration/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-amoutRatioSolute-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/amountRatioOfSoluteToSolventsolute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-amoutRatioSolvent-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/amountRatioOfSoluteToSolventsolvent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-massConcentrationSolute-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/massConcentration/solute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-massConcentrationSolvent-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/massConcentration/solvent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-massFraction-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the componet associated with the mass fraction</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/massFraction/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-massRatioSolute-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/massRatioOfSoluteToSolvent/solute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-massRatioSolvent-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/massRatioOfSoluteToSolvent/solvent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-molalitySolute-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/molality/solute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-molalitySolvent-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/molality/solvent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-moleFraction-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to the component related to the moel fraction</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/moleFraction/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-amountRatioSolute-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/amountRatioOfSoluteToSolvent/solute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-amountRatioSolvent-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/amountRatioOfSoluteToSolvent/solvent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-volumeFraction-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to component described in the volume fraction</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/volumeFraction/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-volumeRatioSolute-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/volumeRatioOfSoluteToSolvent/solute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-volumeRatioSolvent-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/composition/volumeRatioOfSoluteToSolvent/solvent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-moleFractionCryoscopicConstantSolute-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/cryoscopicConstant/moleFractionCryoscopicConstant/solute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-moleFractionCryoscopicConstantSolvent-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/cryoscopicConstant/moleFractionCryoscopicConstant/solvent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-molalityFractionCryoscopicConstantSolute-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/cryoscopicConstant/molalityFractionCryoscopicConstant/solute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-molalityFractionCryoscopicConstantSolvent-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
