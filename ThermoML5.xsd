            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/cryoscopicConstant/molalityFractionCryoscopicConstant/solvent"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-amountDensity-chemicalKeyRef" refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the amount denity component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/density/amountDensity/component"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-apparentMolarEnthlapySolute-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to link the solute component</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/enthalpy/apparentMolarEnthlapy/solute"/>
            <xsd:field xpath="@ref"/>
        </xsd:keyref>
        <xsd:keyref name="dataset-apparentMolarEnthlapySolvent-chemicalKeyRef"
            refer="tml:chemicalIDKey">
            <xsd:annotation>
                <xsd:documentation>key reference to describe the component(s) in the solvent</xsd:documentation>
            </xsd:annotation>
            <xsd:selector
                xpath="dataReport/dataset/property/thermodynamic/enthalpy/apparentMolarEnthlapy/solvent"/>
